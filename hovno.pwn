public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
	//SCMF(100, playerid, -1, "%d %d %d %f %f %f", weaponid, hittype, hitid, fX, fY, fZ);
	if(TestInfiniteAmmo[playerid] == 0)
	{
	    if(!IsPlayerInAnyVehicle(playerid))
	    {
			SetTimerEx("callOnPlayerKeyStateChange", 2500, false, "ii", playerid, GetPlayerAmmo(playerid));
			TestInfiniteAmmo[playerid] = 1;
		}
	}
	if(PlayerInZbrojak[playerid] == 1)
	{
