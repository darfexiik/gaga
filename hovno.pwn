stock NejblizsiObchod(playerid)
{
	new Float:Obchod_Dist, Float:Obchod_DistBliz = 99999.9999, ObchodID_Own = -1;

	for(new i; i < ObchodCount; i ++)
	{
	    if(IsPlayerInArea(playerid, Obchod[i][P_x] - MAX_MAFIE_ZONA, Obchod[i][P_x] + MAX_MAFIE_ZONA, Obchod[i][P_y] - MAX_MAFIE_ZONA, Obchod[i][P_y] + MAX_MAFIE_ZONA))
		{
		    Obchod_Dist = GetPlayerDistanceFromPoint(playerid, Obchod[i][P_x], Obchod[i][P_y], Obchod[i][P_z]);
			if(Obchod_Dist < Obchod_DistBliz)
			{
   				ObchodID_Own = i;
			    Obchod_DistBliz = Obchod_Dist;
			}
		}
	}
	return ObchodID_Own;
}//-1 nenašel }else{ našel
//----
stock MinuteObchodZisk()
{
    for(new i; i < ObchodCount; i ++)
	{
	    if(Obchod[i][P_Prodano] == 1)
	    {
	        if(Obchod[i][P_Zisk] < ((Obchod[i][P_Cena] / 100) * 168))
	        {
	    		Obchod[i][P_Zisk] += (((Obchod[i][P_Cena] / 100) / 60) / 2);
	    		SaveObchod(i);
	    		CHM_Minus((((Obchod[i][P_Cena] / 100) / 60) / 2));
			}
		}
	}
	return 1;
}
