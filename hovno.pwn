						}
					}
				}
				case DIALOG_2D_BUFET:
				{
				    for(new x; x < sizeof(Bufets); x ++)
					{
						if(ID == x)
				   		{
				   		    PlayerTextDrawSetPreviewRot(playerid, Player2DModel[IDs][playerid], -20.000000, 0.000000, 310.000000, 1.000000);
			       			PlayerTextDrawSetPreviewModel(playerid, Player2DModel[IDs][playerid], Bufets[x][Buf_IDs]);

							format(STR, sizeof(STR), "~g~+ %d HP~n~~w~%s$", Bufets[x][Buf_Health], Number(Bufets[x][Buf_Cost]));

							PlayerTextDrawShow(playerid, Player2DModel[IDs][playerid]);
							PlayerTextDrawSetString(playerid, Player2DName[IDs][playerid], STR);
							PlayerTextDrawShow(playerid, Player2DName[IDs][playerid]);
						}
					}
				}
				case DIALOG_2D_AMU:
				{
				    for(new x; x < sizeof(AmuBuy); x ++)
					{
						if(ID == x)
				   		{
				   		    PlayerTextDrawSetPreviewRot(playerid, Player2DModel[IDs][playerid], -20.000000, 0.000000, 310.000000, 1.000000);
			        		PlayerTextDrawSetPreviewModel(playerid, Player2DModel[IDs][playerid], AmuBuy[x][Amu_IDMode]);
							format(STR, sizeof(STR), "%s~n~%s$", TextHack(WeaponName(AmuBuy[x][Amu_IDs])), Number(AmuBuy[x][Amu_Cost]));

							PlayerTextDrawShow(playerid, Player2DModel[IDs][playerid]);
							PlayerTextDrawSetString(playerid, Player2DName[IDs][playerid], STR);
							PlayerTextDrawShow(playerid, Player2DName[IDs][playerid]);
						}
					}
				}
				case DIALOG_2D_ALKOHOL:
				{
				    for(new x; x < sizeof(AlkoholBuy); x ++)
					{
						if(ID == x)
				   		{
				   		    PlayerTextDrawSetPreviewRot(playerid, Player2DModel[IDs][playerid], -20.000000, 0.000000, 310.000000, 1.000000);
			        		PlayerTextDrawSetPreviewModel(playerid, Player2DModel[IDs][playerid], AlkoholBuy[x][Alk_ObjID]);
							format(STR, sizeof(STR), "%s~n~%s$", AlkoholBuy[x][Alk_Name], Number(AlkoholBuy[x][Alk_Cost]));

