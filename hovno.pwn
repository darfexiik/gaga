stock UkazTach(playerid)
{
    if(IsPlayerInAnyVehicle(playerid))
	{
	    switch(Set[S_Tach][playerid])
	    {
	        case 0:
			{
			    PlayerTextDrawHide(playerid, Tachometr[0][playerid]);
			    PlayerTextDrawHide(playerid, Tachometr[1][playerid]);
			    PlayerTextDrawHide(playerid, Tachometr[3][playerid]);
			}
	        case 1:
	        {
	            new STR_Tach[120],VehID = GetPlayerVehicleID(playerid), Float:vehiclehp;
			    //---
			    GetVehicleHealth(VehID, vehiclehp);
				new HP = floatround(vehiclehp);
				HP = HP / 10;
			    //---
			    format(STR_Tach, sizeof(STR_Tach), "~b~Vozidlo: ~w~~h~%s~n~~b~Rychlost: ~w~~h~%d ~y~km/h~n~~b~Palivo: ~w~~h~%d~r~l ~n~~b~Stav: ~w~~h~%d%~y~%", VehName[GetVehicleModel(VehID)-400], GetVehicleSpeed(VehID), Car_Gas[VehID], HP + 1);
			    PlayerTextDrawSetString(playerid,Tachometr[3][playerid], STR_Tach);
			    //---
			    PlayerTextDrawShow(playerid, Tachometr[3][playerid]);
			    PlayerTextDrawHide(playerid, Tachometr[0][playerid]);
			    PlayerTextDrawHide(playerid, Tachometr[1][playerid]);
	        }
	    	case 2:
	    	{
				new STR_Tach[420],VehID = GetPlayerVehicleID(playerid), Float:vehiclehp, STR_stav[80], STR_palivo[80], Stav_barva[20], Palivo_barva[20];
			    //---
			    GetVehicleHealth(VehID, vehiclehp);
				new HP = floatround(vehiclehp);
				HP = (HP + 1) / 10;
				if(HP > 100) HP = 100;
				//----
				switch(HP)
				{
				    case 0 .. 44: format(Stav_barva, sizeof(Stav_barva), "~r~");
				    case 45 .. 74: format(Stav_barva, sizeof(Stav_barva), "~y~");
				    case 75 .. 100: format(Stav_barva, sizeof(Stav_barva), "~g~");
				}
				//----
				switch(Car_Gas[VehID])
				{
				    case 0 .. 44: format(Palivo_barva, sizeof(Palivo_barva), "~r~");
				    case 45 .. 74: format(Palivo_barva, sizeof(Palivo_barva), "~y~");
				    case 75 .. 100: format(Palivo_barva, sizeof(Palivo_barva), "~g~");
				}
				//----
				for(new i; i < HP / 2; i ++)
				{
				    format(STR_stav, sizeof(STR_stav), "%sI",STR_stav);
				}
				for(new i; i < 50 - (HP / 2); i ++)
				{
				    format(STR_stav, sizeof(STR_stav), "%s-",STR_stav);
				}
		        //----
				for(new i; i < Car_Gas[VehID] / 2; i ++)
				{
					format(STR_palivo, sizeof(STR_palivo), "%sI",STR_palivo);
				}
				for(new i; i < 50 - (Car_Gas[VehID] / 2); i ++)
				{
				    format(STR_palivo, sizeof(STR_palivo), "%s-",STR_palivo);
				}
				//---
				new Float:HP_ukaz = (vehiclehp + 1) / 10;
				if(HP_ukaz > 100)	HP_ukaz = 100.0;
				//---
			    format(STR_Tach, sizeof(STR_Tach), "~b~VOZIDLO: ~n~\
				                                    ~w~%s~n~\
													~w~STAV: %s%.1f~n~\
													%s~n~\
													~w~PALIVO: %s%d.0L~n~\
													%s~n~\
													~b~RYCHLOST:~n~\
													~w~%d ~b~KM/H", VehName[GetVehicleModel(VehID)-400], Stav_barva, HP_ukaz, STR_stav, Palivo_barva, Car_Gas[VehID], STR_palivo, GetVehicleSpeed(VehID));
			    PlayerTextDrawSetString(playerid,Tachometr[1][playerid], STR_Tach);
			    //---
   			   	//PlayerTextDrawShow(playerid, Tachometr[0][playerid]);
			    PlayerTextDrawShow(playerid, Tachometr[1][playerid]);
			    PlayerTextDrawHide(playerid, Tachometr[3][playerid]);
			}
		}
	}
	return 1;
}
//------------------------------------------------------------------------------
stock GetVehicleSpeed(vehicleid)
{
	if(vehicleid != INVALID_VEHICLE_ID)
 	{
  		new Float:Pos[3],Float:VS ;
    	GetVehicleVelocity(vehicleid, Pos[0], Pos[1], Pos[2]);
     	VS = floatsqroot(Pos[0]*Pos[0] + Pos[1]*Pos[1] + Pos[2]*Pos[2])*200;

     	//printf("%f %f %f (%d)", Pos[0], Pos[1], Pos[2], floatround(VS,floatround_round));
      	return floatround(VS,floatround_round);
    }
    return INVALID_VEHICLE_ID;
}
//------------------------------------
stock SetVehicleSpeed(vehicleid, Float:speed)
{
	new Float:x1, Float:y1, Float:z1, Float:x2, Float:y2, Float:z2, Float:a;
	GetVehicleVelocity(vehicleid, x1, y1, z1);
	GetVehiclePos(vehicleid, x2, y2, z2);
	GetVehicleZAngle(vehicleid, a);
	a = 360 - a;
	x1 = (floatsin(a, degrees) * (speed/100) + floatcos(a, degrees) * 0 + x2) - x2;
	y1 = (floatcos(a, degrees) * (speed/100) + floatsin(a, degrees) * 0 + y2) - y2;
	SetVehicleVelocity(vehicleid, x1, y1, z1);
}
