stock DSQ_Seskok(playerid)
{
    new se = GetPVarInt(playerid,"SeskokID");
    if(Seskok[se][Seskok_Hrac][playerid] == 1 && SeskokPlayer[playerid] == 1)
	{
	    Seskok[se][Seskok_Zavodnici]        --;
		SeskokPlayer[playerid] 				= 0;
		Seskok[se][Seskok_Hrac][playerid] 	= 0;
		SetPlayerVirtualWorldEx(playerid, 	0);
		DisablePlayerRaceCheckpoint(playerid);
		Cheat[ZbraneProtekce][playerid] 	= 2;
		Hrac[NPC_CP][playerid] 				= 1;
		DejHnedGun(playerid);
		SetPlayerWorldBounds(playerid,9999*9999,-9999*9999,9999*9999,-9999*9999);
		TogglePlayerControllable(playerid,1);

		BackToport(playerid, 1);
		new ID;
		ForMaxPlayer(i)
		{
			if(Seskok[se][Seskok_Hrac][i] == 1 && SeskokPlayer[i] == 1)
 			{
 			    ID ++;
 			}
		}
		if(ID == 0)
		{
		    ZrusSeskok(se);
		}
	}
	return 1;
}

