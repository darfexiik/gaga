                //---
				SaveRaceTop(RaceID);
		        return 1;
			}else{
			    //---
			    if(PlayerRaceTime[playerid] < RaceTopTime[RaceID][ID])
			    {
				    //---
					for(new x = ID; x < MAX_RACE_TOP - 1; x ++)
					{
					    format(RaceTopName[RaceID][x], MAX_PLAYER_NAME, "%s", RaceTopName[RaceID][x + 1]);
	        			RaceTopTime[RaceID][x] = RaceTopTime[RaceID][x + 1];
					}
					//---
					if(IsPlayerInTop(playerid, RaceID) != -1) print("nevymazal");
					//---
					for(new x = MAX_RACE_TOP - 1; x > i; x --)
			        {
			            if(x > 0)
			            {
							format(RaceTopName[RaceID][x], MAX_PLAYER_NAME, "%s", RaceTopName[RaceID][x - 1]);
							RaceTopTime[RaceID][x] = RaceTopTime[RaceID][x - 1];
						}
					}
			        format(RaceTopName[RaceID][i], MAX_PLAYER_NAME, "%s", PlayerName(playerid));
			        RaceTopTime[RaceID][i] = PlayerRaceTime[playerid];
			        //---
			        new Minuta,Sekunda,Milisecond;
					MilisecondToTime(PlayerRaceTime[playerid],Minuta,Sekunda,Milisecond);
					//---
			        SPIBF(100,playerid, 3,"Dojel jste v case: %02d:%02d:%02d~n~Umistil jste se na %d. miste v TOP 10.", Minuta, Sekunda, Milisecond, i + 1);
	                //---
					SaveRaceTop(RaceID);
					return 1;
				}else{
				    //---
				 	new Minuta,Sekunda,Milisecond;
					MilisecondToTime(PlayerRaceTime[playerid],Minuta,Sekunda,Milisecond);
				    //---
					SPIBF(100,playerid, 3,"Dojel jste v case: %02d:%02d:%02d", Minuta, Sekunda, Milisecond);
					return 1;
				}
			}
	    }
	}
	//---
 	new Minuta,Sekunda,Milisecond;
	MilisecondToTime(PlayerRaceTime[playerid],Minuta,Sekunda,Milisecond);
    //---
	SPIBF(100,playerid, 2,"Dojel jste v case: %02d:%02d:%02d", Minuta, Sekunda, Milisecond);
	return 0;
}
