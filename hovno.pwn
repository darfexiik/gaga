public OnPlayerDeath(playerid, killerid, reason)
{
    //--------------------------------------------------------------------------
    Cheat[ActiveAntiCheat][playerid] = 1;
    Cheat[ZbraneProtekce][playerid] = 3;
    Cheat[FlyProtekce][playerid] = 5;
    Cheat[FlyHack][playerid] = 0;
    Cheat[PortyProtekce][playerid] = 3;
    PlayerVybralGUN[playerid] = 0;
    //--------------------------------------------------------------------------
    if(Hrac[Zamestnani][playerid] == POLDA)
	{
	    SetPlayerWantedLevelEx(killerid, GetPlayerWantedLevel(killerid) + 5);
	    SCM(killerid,0x0000FFAA,"Spáchal jste zloèin: Zabití policisty. (+ 5 WL)");
	}
	//--------------------------------------------------------------------------
    switch(Hrac[Zamestnani][playerid])
	{
  		case RYBAR: ZrusAllMise(playerid, 6);
  		case EXECUTOR: ZrusAllMise(playerid, 2);
		default: ZrusAllMise(playerid, 7);
	}
    //--------------------------------------------------------------------------
    Sav[Skin][playerid] = GetPlayerSkin(playerid);
    //--------------------------------------------------------------------------
    if(PlayerInSkola[playerid] == 1)
	{
	    SPIB2(playerid, "~r~Autoskola zrusena!", 5);
		ZrusPlayerSkola(playerid);
	}
    //--------------------------------------------------------------------------
    RaceNul(playerid);
    DSQ_Seskok(playerid);
    Hrac[DeathON][playerid] = 1;
    //--------------------------------------------------------------------------
    Sav[LCity][playerid] = IsPlayerInCityRace(playerid);
    //--------------------------------------------------------------------------
    if(reason != 255)
	{
		if(reason != 200)
		{
			if(reason != 201)
			{
				if(reason != 54)
				{
				    if(ServerCheat[C_FAKE_KILL] == 1)
				    {
				        if((gettime() - Cheat[PlayerLastKill][playerid]) < 3)
				        {
				            Cheat[KillCount][playerid] ++;
				            if(Cheat[KillCount][playerid] >= 3)
				            {
				                SCMTAFL(70,0xFF0000AA,"Hráè %s byl vyhozen z dùvodu: Fake Kill",PlayerName(playerid));
		                    	Log_Write(100,playerid,"Hráè %s byl vyhozen z dùvodu: Fake Kill",PlayerName(playerid));
								SetTimerEx("Kicked",900,0,"i",playerid);
				            }
				        }else{
							Cheat[PlayerLastKill][playerid] = gettime();
                            Cheat[KillCount][playerid] = 0;
