			SCMTA(0x00FFFF88,string);
			Log_Write(128,DM[dm][DM_ASI_WIN_ID],"(DM) Vítìzem deathmatche {FFFFFF}%s{00FFFF} je {FFFFFF}%s{00FFFF} se skóre %d. Výhra je %d$.",DM[dm][DM_NAME],PlayerName(DM[dm][DM_ASI_WIN_ID]),DM[dm][DM_Score],DM[dm][DMCastka]*DM[dm][DM_DMteri]);
		}else{
			format(string,sizeof(string),"Deathmatch{FFFFFF} %s {00FFFF}nikdo nevyhrál",DM[dm][DM_NAME]);
			SCMTA(0x00FFFF88,string);
		}
		DM[dm][DM_ASI_WIN_ID] = -1;
	}
	return 1;
