stock ChatColors(string[])
{
	new len = strlen(string);
	for(new i; i < len;i++)
	{
		if(string[i] == '[')
		{
			new bool:hex,count;
			for(new x = 1; x < 7; x++)
			{
				if((string[i+x] >= '0' && string[i+x] <= '9') || (string[i+x] >= 'a' && string[i+x] <= 'f') || (string[i+x] >= 'A' && string[i+x] <= 'F')){
				count++;
				}else{
				count++;
				hex = true;
				}
			}
			if(count == 6 && string[i+7] == ']')
			{
				if(hex == true)
				{
					for(new c = 1;c < 7;c++)
					{
				 		string[i+c] = 'F';
					}
				}
				string[i] = '{';
				string[i+7] = '}';
			}

		}
	}
}

stock TextHack(const Text[])
{
	new STR[128];
	format(STR, sizeof(STR), "%s", Text);
	for(new i; i < sizeof(STR); i ++)
	{
	    switch(STR[i])
	    {
	        case 'ì', 'é': STR[i] = 'e';
	        case 'š': STR[i] = 's';
	        case 'è': STR[i] = 'c';
	        case 'ø': STR[i] = 'r';
	        case 'ž': STR[i] = 'z';
	        case 'ý': STR[i] = 'y';
	        case 'á': STR[i] = 'a';
	        case 'í': STR[i] = 'i';
	        case 'ú', 'ù': STR[i] = 'u';

	        case 'Ì', 'É': STR[i] = 'E';
	        case 'Š': STR[i] = 'S';
	        case 'È': STR[i] = 'C';
	        case 'Ø': STR[i] = 'R';
	        case 'Ž': STR[i] = 'Z';
	        case 'Ý': STR[i] = 'Y';
	        case 'Á': STR[i] = 'A';
	        case 'Í': STR[i] = 'I';
	        case 'Ú', 'Ù': STR[i] = 'U';
	    }
	}
	return STR;
}

stock HexToInt(string[])
{
	if (string[0]==0) return 0;
 	new i;
  	new cur=1;
  	new res=0;
  	for (i=strlen(string);i>0;i--)
  	{
    	if (string[i-1]<58) res=res+cur*(string[i-1]-48); else res=res+cur*(string[i-1]-65+10);
    	cur=cur*16;
  	}
  	return res;
}

stock RGB( red, green, blue, alpha )
{
	return (red * 16777216) + (green * 65536) + (blue * 256) + alpha;
}

stock TextColorHEX(const _texthex[])
{
    new red[3], green[3], blue[3], alpha[3], T_Color;
	if(_texthex[0] == '0' && _texthex[1] == 'x')
	{
		format(red, sizeof(red), "%c%c", _texthex[2], _texthex[3]);
		format(green, sizeof(green), "%c%c", _texthex[4], _texthex[5]);
		format(blue, sizeof(blue), "%c%c", _texthex[6], _texthex[7]);
		if(_texthex[8] != '\0')		format(alpha, sizeof(alpha), "%c%c", _texthex[8], _texthex[9]);
		else						alpha = "FF";

		T_Color = RGB(HexToInt(red), HexToInt(green), HexToInt(blue), HexToInt(alpha));
  	}
  	return T_Color;
}


stock TextColorARGB(const _texthex[])
{
    new red[3], green[3], blue[3], alpha[3], T_Color;
	if(_texthex[0] == '0' && _texthex[1] == 'x')
	{
		format(red, sizeof(red), "%c%c", _texthex[2], _texthex[3]);
		format(green, sizeof(green), "%c%c", _texthex[4], _texthex[5]);
		format(blue, sizeof(blue), "%c%c", _texthex[6], _texthex[7]);
		if(_texthex[8] != '\0')		format(alpha, sizeof(alpha), "%c%c", _texthex[8], _texthex[9]);
		else						alpha = "FF";

		T_Color = RGB(HexToInt(alpha), HexToInt(red), HexToInt(green), HexToInt(blue));
  	}
  	return T_Color;
}

