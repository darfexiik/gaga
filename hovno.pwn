stock GetRegionID(Float:x, Float:y, Vyjimka = 0)
{
	for(new r=0; r<MAX_REGIONS; r++)
	{
		if(x >= Regions[r][region_minx] && x <= Regions[r][region_maxx] && y >= Regions[r][region_miny] && y <= Regions[r][region_maxy])
		{
			if(Vyjimka == 1)
			{
			    if(r >= 300) return r;
			}else{
				return r;
			}
		}
	}
	return INVALID_REGION;
}
//--------------------------------------
stock GetRegionName(regionid)
{
	new regionname[MAX_NAME_SIZE] = " ";
	if(regionid != INVALID_REGION) format(regionname, sizeof(regionname), "%s", Regions[regionid][region_name]);
	else format(regionname, sizeof(regionname), "San Andreas");
	return regionname;
}
