stock GivePlayerMoneyEx(playerid, sMoneyV, Protekce = 0)
{
    Log_Write(100,playerid,"(MONEY) %s", Number(sMoneyV));
    Cheat[ActiveAntiCheat][playerid] = 3;
    if(Protekce == 1)
    {
        Sav[P_Money][playerid] += sMoneyV;
	    return GivePlayerMoney(playerid, sMoneyV);
    }
	//---
	if(Sav[PlatebniKarta][playerid] == 1)
	{
	    if(Sav[PlatbaOnOff][playerid] == 1)
	    {
			Sav[Banka][playerid] += sMoneyV;
			return 1;
	    }else{
	        Sav[P_Money][playerid] += sMoneyV;
	        return GivePlayerMoney(playerid, sMoneyV);
	    }
	}else{
	    Sav[P_Money][playerid] += sMoneyV;
	    return GivePlayerMoney(playerid, sMoneyV);
	}
}
