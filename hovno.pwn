CMD:unlock(playerid)
{
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER)         return SCM(playerid,0xff0000AA,"Musíš být øidiè vozidla");
	if(ZamekCar[GetPlayerVehicleID(playerid)] == 1)
	{
 		GameTextForPlayer(playerid,"~b~~h~~h~~h~ODEMKNUTO",2*1000,4);

	    ZamekCar[GetPlayerVehicleID(playerid)] = 0;
	    ForMaxPlayer(i)
		{
			SetVehicleParamsForPlayer(GetPlayerVehicleID(playerid),i, 0, 0);
		}

		PlayerPlaySound(playerid, 1057, 0.0000, 0.0000, 0.0000);
	}
	Log_Write(100,playerid,"Hráè %s odemknul %s (%d)", PlayerName(playerid), VehName[GetVehicleModel(GetPlayerVehicleID(playerid))-400], GetPlayerVehicleID(playerid));
	return 1;
}
