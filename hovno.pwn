public OnUnoccupiedVehicleUpdate(vehicleid, playerid, passenger_seat)
{
    if (vehicleid < PosledniAuto && !passenger_seat && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT
                && GetVehicleDistanceFromPoint (vehicleid, VehiclePosX[vehicleid], VehiclePosY[vehicleid], VehiclePosZ[vehicleid]) > 50.0)
    {
    	new Float: x, Float: y, Float: z;

		GetXYZInFrontOfPlayer(playerid, x, y, z, 5.0);
  		if (GetVehicleDistanceFromPoint (vehicleid, x, y, z) < 5.0)
    	{
    	    switch(GetVehicleModel(vehicleid))
    	    {
    	        case 606, 607, 608, 610, 611, 591, 590, 584, 569, 450, 435: {}
    	        default:
    	        {
    	            /*if(ServerCheat[C_PORT_VEHICLE] == 1)
		    	    {
		    	        if(OnlyAdmin() == 0)
						{
		    	        	SCMTAFL(100,0xFF0000AA,"Hráè %s byl vyhozen z dùvodu: Spawn Hack",PlayerName(playerid));
		    	        	Log_Write(100,playerid,"Hráè %s byl vyhozen z dùvodu: Spawn Hack",PlayerName(playerid));
							SetTimerEx("Kicked",900,0,"i",playerid);
						}
					}*/
					new regionid = PlayerActiveRegion[playerid];
					SCMAT(128,0x666666AA,"*(A)* Hráè %s (%d) si zøejmì pøivolal %s (%d) v oblasti %s", PlayerName(playerid), playerid, VehName[GetVehicleModel(vehicleid)-400], vehicleid, GetRegionName(regionid));
					SetVehicleToRespawn(vehicleid);
					return 0;
    	        }
    	    }
        }
	}
	GetVehiclePos(vehicleid, VehiclePosX[vehicleid], VehiclePosY[vehicleid], VehiclePosZ[vehicleid]);
	return 1;
}
