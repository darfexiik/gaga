public OnObjectMoved(objectid)
{
	ForMaxPlayer(i)
    {
        if(Hrac[Zamestnani][i] == FARMAR)
        {
            if(objectid == ObjectsForMise[i])
			{
			    if(MiseOn[i] == 1)
			    {
			        FarmarObili[i] ++;
			        MiseOn[i] = 0;
			        DestroyObject(ObjectsForMise[i]);
			        FarmarMise(i);
			        return 1;
			    }
			}
        }
    }
    for(new i; i < MAX_FALL; i ++)
	{
	    if(FallObjectID[i] == objectid)
	    {
	        switch(Event[EventDruh])
	    	{
	    	    case EVENT_FALLOUT:
	    	    {
			        new Float:oX, Float:oY, Float:oZ;
					GetObjectPos(FallObjectID[i], oX, oY, oZ);
					switch(FallObjectStav[i])
					{
						case 0: MoveObject(FallObjectID[i], oX, oY, oZ + 0.05, FALL_OUT_SPEED, 0.0000,  10.0000, 0.0000);
						case 1: MoveObject(FallObjectID[i], oX, oY, oZ - 0.05, FALL_OUT_SPEED, 0.0000, -10.0000, 0.0000);
						case 2: MoveObject(FallObjectID[i], oX, oY, oZ + 0.05, FALL_OUT_SPEED, 0.0000,  10.0000, 0.0000);
						case 3: MoveObject(FallObjectID[i], oX, oY, oZ - 0.05, FALL_OUT_SPEED, 0.0000, -10.0000, 0.0000);
						case 4: MoveObject(FallObjectID[i], oX, oY, oZ + 0.05, FALL_OUT_SPEED, 0.0000,  0.0000, 0.0000);
						case 5:	MoveObject(FallObjectID[i], oX, oY, 100.0000, 10, 0.0000,  0.0000, 0.0000);
						case 6:
						{
						    FallObjectActive[i] = 0;
						    FallObjectStav[i]   = 0;
						    DestroyObject(FallObjectID[i]);
						}
					}

					FallObjectStav[i] ++;
				}
                case EVENT_PLOSINY:
                {
                    new Float:oX, Float:oY, Float:oZ, RanSpeed = 2 + random(5);
					GetObjectPos(FallObjectID[i], oX, oY, oZ);
					if(oX == (FALL_OUT_X + PLOSINY_DISTANCE))
					{
                        MoveObject(FallObjectID[i], (FALL_OUT_X - PLOSINY_DISTANCE), oY, oZ, RanSpeed, 0.0000,  0.0000, 0.0000);
					}
					if(oX == (FALL_OUT_X - PLOSINY_DISTANCE))
					{
					    MoveObject(FallObjectID[i], (FALL_OUT_X + PLOSINY_DISTANCE), oY, oZ, RanSpeed, 0.0000,  0.0000, 0.0000);
					}
				}
			}
		}
	}

	if(Event[EventVytvoren] == 1 && Event[EventDruh] == EVENT_PLOSINY)
	{
		new Member;
		ForMaxPlayer(i)
		{
	 		if(PlayerEvent[i] == 1)
			{
	            Member ++;
		    }
		}
		if(Member == 0)
		{
		    SCMTAF(120, 0xFF0000FF, "Event %s nikdo nevyhrál", Event[EventName]);
		    ZrusEvent();
		}
	}
	return 1;
}

