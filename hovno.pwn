public OnPlayerClickPlayer(playerid, clickedplayerid, source)
{
    new ID = clickedplayerid;

    new JOBID;
    for(new i; i < JOBCount; i ++)
	{
		if(JOB[i][JOB_ID] == Sav[Last_Job][ID])
		{
  			JOBID = i;
		}
 	}
	new Hodina,Minuta,MafieID;
	MinuteToHodin(Sav[Minuty][ID],Hodina,Minuta);

	MafieID = PlayerMafieID[ID];

	format(MAX_STR, sizeof(MAX_STR), "%sNick:{84E812} %s\n", MAX_STR, PlayerName(ID));
	format(MAX_STR, sizeof(MAX_STR), "%sID:{84E812} %d\n", MAX_STR, ID);
    format(MAX_STR, sizeof(MAX_STR), "%sPeníze:{84E812} %s$\n", MAX_STR, Number(Sav[P_Money][ID]));
    format(MAX_STR, sizeof(MAX_STR), "%sBanka:{84E812} %s$\n", MAX_STR, Number(Sav[Banka][ID]));
	format(MAX_STR, sizeof(MAX_STR), "%sÚmrtí:{84E812} %d\n", MAX_STR, Sav[Zabit][ID]);
	format(MAX_STR, sizeof(MAX_STR), "%sZabití:{84E812} %d\n", MAX_STR, Sav[Zabil][ID]);
	//format(MAX_STR, sizeof(MAX_STR), "%sAdmin:{84E812} %d\n", MAX_STR, AdminAnoNe(Sav[Admin][ID]));
    format(MAX_STR, sizeof(MAX_STR), "%sDatum Registrace:{84E812} %s\n", MAX_STR, BanDate(Sav[First_Log][ID],1));
    format(MAX_STR, sizeof(MAX_STR), "%sNahráno:{84E812} %02d:%02d hodin\n", MAX_STR, Hodina, Minuta);
    if(JOBID == NEZAMESTNANY)
	{
	    format(MAX_STR, sizeof(MAX_STR), "%sPovolání:{84E812} Nezamìstnaný\n", MAX_STR);
	}
	else
	{
		format(MAX_STR, sizeof(MAX_STR), "%sPovolání:{84E812} %s\n", MAX_STR, JOB[JOBID][JOB_Name]);
        //format(MAX_STR, sizeof(MAX_STR), "%sHodnost:{84E812} %s\n", MAX_STR, Hodnosti(ID));
	}
	if(MafieID != -1)
	{

		format(MAX_STR, sizeof(MAX_STR), "%sGang:{84E812} %s\n", MAX_STR, Mafie[MafieID][MafieName]);
	}
	else format(MAX_STR, sizeof(MAX_STR), "%sNení v žádném gangu\n", MAX_STR);

	new Nadpis[100];
	format(Nadpis, sizeof(Nadpis), "Informace hráèe %s (%d)", PlayerName(ID), ID);
    ShowPlayerDialogEx(playerid,DIALOG_TABINFO, DIALOG_STYLE_LIST, Nadpis, MAX_STR, "Zavøít", "");
	return 1;
}
