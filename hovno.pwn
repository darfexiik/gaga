CMD:kill(playerid)
{
    if(Sav[VezeniTime][playerid] > 0)          					        return SCM(playerid, 0xFF0000AA, "Vìzeò nemùže spáchat sebevraždu!");
    else if(PlayerInDM[playerid] == 1 && PlayerBox[playerid] == 1)      return SCM(playerid, 0xFF0000AA, "V minihøe nemùže spáchat sebevraždu!");
    else if(PlayerStroj[playerid] != G_NOTHING) 						return SCM(playerid,0xff0000AA,"Máte nìco na práci!");
	else if(PlayerInDM[playerid] == 1)                                  return SCM(playerid,0xff0000AA,"Máte nìco na práci!");
	else if(PlayerInZbrojak[playerid] == 1)                             return SCM(playerid,0xff0000AA,"Máte nìco na práci!");

	new weapons[2];
	for(new i; i < MAX_SLOT_GUN; i++)
	{
	    GetPlayerWeaponData(playerid, i, weapons[0], weapons[1]);
	    if(weapons[0] > 0 && weapons[1] > 0)

	    {
           Log_Write(100,playerid,"(DEATH) %s(%d)", WeaponName(weapons[0]), weapons[1]);
		}
	}
    //Log_Serverf(100,"Hráè %s spáchal sebevraždu", PlayerName(playerid));

	SetPlayerHealEx(playerid, 0);

	Sav[Zabit][playerid] ++;
	Sav[Sebevrazd][playerid] ++;
    return 1;
}
