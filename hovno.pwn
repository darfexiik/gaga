{
    if(Hrac[Zamestnani][playerid] != ODTAHOVKA)						return SCM(playerid,0xff0000AA,"Nejste zamìstnán jako mechanik");
	else if(OdtahMoto[playerid] == 0)                           	return SCM(playerid,0xff0000AA,"Pøíkaz fuel nelze použít");
	else if(MiseOn[playerid] == 0)                              	return SCM(playerid,0xff0000AA,"Nemáte Zapnutou misi");
	else if(GetVehicleModel(GetPlayerVehicleID(playerid)) != 525) 	return SCM(playerid,0xff0000AA,"Nejste v odtahovém vozidle");

	new trailer = OdtahVehicle[playerid];
    if(PlayerDistaceToVehicle(playerid, trailer) < 30)
    {
        DisableSipka(trailer);
        OdtahMoto[playerid] = 0;
        Car_Gas[trailer] = 10;
        ZacatekMise[playerid] = 0;
        OdtahVehicle[playerid] = -1;
    	MiseOn[playerid] = 0;
		Vyplaty(playerid);
		OdtahovakaMise(playerid);
    }else SCM(playerid,0xff0000AA,"Jste daleko od vozidla");
	return true;
}

