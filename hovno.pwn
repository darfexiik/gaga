//------------------------------------------------------------------------------
stock HodinaTimer()
{
	new MafiePoceteDolar[MAX_MAFIE];
    for(new i = 1; i < ObchodCount; i ++)
	{
	    if(Obchod[i][P_Prodano] == 1)
	    {
	        Obchod[i][P_Najem] --;
	        if(Obchod[i][P_Najem] <= 0)
	        {
	            ProdejObchod(i);
	            SaveObchod(i);
	        }
	    }
	    new MafieID = Obchod[i][MafieObchodID];
     	if(MafieID != -1)
      	{
            if(Mafie[MafieID][MafieKoupena] == 1)
            {
                Mafie[MafieID][MafieTrezor] += 6500;
                MafiePoceteDolar[MafieID] += 6500;
			}
		}
	}
	for(new i = 0; i < MafieCount; i ++)
	{
	    if(Mafie[i][MafieKoupena] == 1)
	    {
	        for(new x; x < MAX_MAFIE_PLAYER; x ++)
			{
			    new Hodnost = MPHodnost[x][i];
				if(strcmp(MP[x][i], "INVALID", false) != 0 && Hodnost != -1)
				{
				    //printf("mafie %d hodnost %d peníze %s$", i, Hodnost - 1, Number(MafieHodnost[Hodnost][i]));
				    MafieVyplataGIVE[x][i] += MafieHodnost[Hodnost - 1][i];
				    Mafie[i][MafieTrezor] -= MafieHodnost[Hodnost - 1][i];
				}
			}
			CHM_Minus(MafiePoceteDolar[i]);
			Log_Mafie(120,i,"Gang obdržel výpalné v celkové výši %s$.", Number(MafiePoceteDolar[i]));
			UlozMafii(i);
	    }
	}
	for(new i; i < PoleCount; i ++)
	{
 		if(Pole[i][PoleKoupeno] == 1)
		{
			Pole[i][PolePronajem] --;
			if(Pole[i][PolePronajem] <= 0)
			{
			    ProdejPole(i);
				UlozPole(i);
			}
		}
	}
	return 1;
}
