	Sav[First_Log][playerid] 			= gettime();
	Sav[NastaveniTag][playerid]         = 0;
	Sav[TagCol][playerid]   			= 10;
	//-----------------------------------
	for(new p; p < MAX_SLOT_GUN; p ++)
	{
	    SavAmmo[p][playerid] = 0;
	    SavGun[p][playerid] = 0;
	}
	//-----------------------------------
	for(new p; p < MAX_SAV_BODY; p ++)
	{
