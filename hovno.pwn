public RaceTimer(ra)
{
    if(Race[ra][RACE_Created] == 1 && Race[ra][RACE_Winer] == 1)
    {
        new STR[350];
	    ForMaxPlayer(i)
		{
			if(Race[ra][RACE_Hrac][i] == 1 && zavod[i] == 1)
			{
			    new cp = Race[ra][PrvniCP];
			    if(PlayerCPc[i] == cp)
			    {
			    	PlayerDistance[i] = GetPlayerDistanceFromPoint(i, rcp_X[ra][cp - 1], rcp_Y[ra][cp - 1], rcp_Z[ra][cp - 1]);
				}else{
				    new CP = PlayerCPc[i];
				    PlayerDistance[i] = GetPlayerDistanceFromPoint(i, rcp_X[ra][CP - 1], rcp_Y[ra][CP - 1], rcp_Z[ra][CP - 1]);
				    PlayerDistance[i] += GetPlayerDistanceFromPoint(i, rcp_X[ra][cp - 1], rcp_Y[ra][cp - 1], rcp_Z[ra][cp - 1]);
				}
				KolikJeZaRacerem[i] = 0;
			}
		}
		//---
		ForMaxPlayer(i)
		{
			if(Race[ra][RACE_Hrac][i] == 1 && zavod[i] == 1)
			{
			    //---
			    ForMaxPlayer(x)
				{
					if(Race[ra][RACE_Hrac][x] == 1 && zavod[x] == 1)
					{
					    if(x != i)
					    {
						    if(PlayerDistance[i] < PlayerDistance[x])
						    {
						        KolikJeZaRacerem[i] ++;
						    }
						}
					}
				}
				new Misto = (Race[ra][RACE_Zavodnici] - KolikJeZaRacerem[i]);
				PoradiPlayer[Misto - 1][ra] = i;

			}
		}
	    //----------------------------------------------------------------------
		ForMaxPlayer(i)
		{
			if(Race[ra][RACE_Hrac][i] == 1 && zavod[i] == 1)
			{
				new Minuta,Sekunda,Milisecond;
				MilisecondToTime((GetTickCount() - PlayerRaceTime[i]),Minuta,Sekunda,Milisecond);
				//--------------------------------------------------------------
				format(STR,sizeof(STR),"CP: ~g~~h~%d~w~/~g~~h~%d~w~, Cas: ~g~~h~%02d~w~:~g~~h~%02d", PlayerCPc[i] - 1, Race[ra][RACE_cislo], Minuta, Sekunda);
				//--------------------------------------------------------------
				for(new x; x < Race[ra][RACE_Zavodnici]; x ++)
			    {
			        new ID = PoradiPlayer[x][ra];
			        if(strcmp(PlayerName(i), PlayerName(ID), false) == 0) 	format(STR,sizeof(STR),"%s~n~~g~~h~%s %d.",STR,PlayerName(ID), x + 1);
			        else													format(STR,sizeof(STR),"%s~n~%s %d.",STR,PlayerName(ID), x + 1);
			    }
			    format(STR,sizeof(STR),"%s~n~",STR);
				//--------------------------------------------------------------
			    PlayerTextDrawSetString(i, StadionTD[i], STR);
			    PlayerTextDrawShow(i, StadionTD[i]);
			}
		}
		//---
		SetTimerEx("RaceTimer", 1000, 0, "i", ra);
	}else{
	    ForMaxPlayer(i)
		{
			if(Race[ra][RACE_Hrac][i] == 1 && zavod[i] == 1)
			{
			    PlayerTextDrawHide(i, StadionTD[i]);
			}
		}
	}
	return 1;
}
