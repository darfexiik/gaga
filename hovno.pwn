stock KodMesice(Year, Month)
{
	switch(Month)
	{
	    case 1:
	    {
	        if(PrestupnyRok(Year) == 1) return 6;
	        else return 5;
		}
	    case 2:
	    {
			if(PrestupnyRok(Year) == 1) return 2;
	        else return 1;
		}
	    case 3: return 2;
	    case 4: return 5;
	    case 5: return 0;
	    case 6: return 3;
	    case 7: return 5;
	    case 8: return 1;
	    case 9: return 4;
	    case 10: return 6;
	    case 11: return 2;
	    case 12: return 4;
	}
	return -1;
}

stock VypocetKoduRoku()
{
    new Year, Month, Day, PomocnyVyp;
	getdate(Year, Month, Day);
	//-----
	Year = Year - 2000;
	PomocnyVyp = Year / 4;
	//-----
	PomocnyVyp = Year + PomocnyVyp;
	//-----
	for(new i; i < Year; i ++)
	{
	    if((PomocnyVyp - (i * 7)) < 7)
	    {
	        return PomocnyVyp - (i * 7);
	    }
	}
	return 0;
}

stock VypocetDne()
{
    new Year, Month, Day, PomocnyVyp;
	getdate(Year, Month, Day);
	//-----
	PomocnyVyp = KodMesice(Year, Month) + Day + VypocetKoduRoku();
	//-----
	if(PomocnyVyp > 7)
	{
	    for(new i; i < PomocnyVyp; i ++)
		{
		    if((PomocnyVyp - (i * 7)) < 7)
		    {
		        return PomocnyVyp - (i * 7);
		    }
		}
	}
	return PomocnyVyp;
}
