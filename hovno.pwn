stock SaveRaceTop(RaceID)
{
    new string[50];
	new Data[50];
	format(string,50,"Races/zavod%d.txt",RaceID);
	if(fexist(string))
	{
	    new File:PFile = fopen(string,io_write);
	    //--
		for(new i; i < MAX_RACE_TOP; i ++)
		{
		   	format(Data,sizeof(Data),"%s %d\r\n",RaceTopName[RaceID][i],RaceTopTime[RaceID][i]);
			fwrite(PFile,Data);
		}
		//--
		fclose(PFile);
	}
	return 1;
}
