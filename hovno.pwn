CMD:color(playerid,params[])
{
	new barva[2];
	if(HaveVIP(playerid) == 0)						return SCM(playerid,0xff0000AA,"Nejste vlastníkem VIP úètu");
	else if(!IsPlayerInAnyVehicle(playerid))		return SCM(playerid,0xff0000AA,"Nejste ve vozidle");
    else if(sscanf(params,"ii",barva[0],barva[1])) 	return SCM(playerid,0xff0000AA,"Nápovìda: /color (BARVA 1] (BARVA 2)");
    else if(barva[0] > 255 || barva[0] < 0) 		return SCM(playerid,0xff0000AA,"Nápovìda: /color (BARVA 1] (BARVA 2)");
    else if(barva[1] > 255 || barva[1] < 0) 		return SCM(playerid,0xff0000AA,"Nápovìda: /color (BARVA 1] (BARVA 2)");
    if(IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		ChangeVehicleColor(GetPlayerVehicleID(playerid),barva[0],barva[1]);
	  	SPIB(playerid, "Vozidlo uspesne prebarveno",3);
	  	SetTimerEx("OnVehicleRespray", 1000, false, "iiii", playerid, GetPlayerVehicleID(playerid),barva[0],barva[1]);
	}else return SCM(playerid,0xff0000AA,"Musíte být øidiè vozidla.");
	return true;
}

CMD:vr(playerid)
{
    new Float: vehiclehp;
	GetVehicleHealth(GetPlayerVehicleID(playerid), vehiclehp);

    if(HaveVIP(playerid) == 0)							return SCM(playerid,0xff0000AA,"Nejste vlastníkem VIP úètu");
	else if(!IsPlayerInAnyVehicle(playerid))			return SCM(playerid,0xff0000AA,"Nejste ve vozidle");
	else if(vehiclehp == 999)                           return SCM(playerid,0xff0000AA,"Toto vozidlo není poškozené");
	else if(zavod[playerid] == 1)               		return SCM(playerid,0xff0000AA,"V závodì nelze opravit vozidlo");
	else if(PlayerEvent[playerid] == 1)                 return SCM(playerid,0xff0000AA,"V eventu nelze opravit vozidlo");

	if(OpravitCas[playerid] > 0)
	{
	    SCMF(128,playerid,0xFF0000AA,"Tento pøíkaz lze použít za 00:%02d minut",OpravitCas[playerid]);
	    return 1;
	}
	RepairVehicle(GetPlayerVehicleID(playerid));
	SetVehicleHealth(GetPlayerVehicleID(playerid),999);
    SPIB(playerid, "Vozidlo bylo uspesne opraveno",3);
    OpravitCas[playerid] = 30;
	return true;
}

