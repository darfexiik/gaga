	    case EVENT_TEAM_DM: format(STR, sizeof(STR), "Team Deathmatch");
		case EVENT_FALLOUT: format(STR, sizeof(STR), "Fallout");
		case EVENT_PLOSINY: format(STR, sizeof(STR), "Pohybující pološiny");
		case EVENT_DERBY:   format(STR, sizeof(STR), "Derby");
		case EVENT_BLUDISTE:format(STR, sizeof(STR), "Bludištì");
	}
	return STR;
}

stock EventCreate(playerid)
{
    if(Event[EventVytvoren] == 1)       return SCM(playerid, 0xFF0000FF, "Event již je vytvoøen! /zrusevent");

    SCMTAF(120,0xFF0000FF,"Administrátor %s vytvoøil %s: {FFFFFF}%s{FF0000} /event", PlayerName(playerid), EventDruhName(Event[EventDruh]), Event[EventName]);
    Event[EventPortON] = 1;
    Event[EventVytvoren] = 1;

    new mstr[30],mstr1[30],mstr2[30],mstr3[40];

	format(mstr,sizeof(mstr),"Nazev: ~y~%s", Event[EventName]);
	TextDrawSetString(TDEVENTA[0],mstr);

	format(mstr1,sizeof(mstr1),"Vyhra: ~y~%s $", Tecka(Event[EventVyhra]));
	TextDrawSetString(TDEVENTA[2],mstr1);

	format(mstr2,sizeof(mstr2),"Cil: ~y~Vitezstvi");
	TextDrawSetString(TDEVENTA[3],mstr2);

	format(mstr3,sizeof(mstr3),"Vytvoril: ~y~%s",PlayerName(playerid));
	TextDrawSetString(TDEVENTA[4],mstr3);
	ShowTDEvent();

	switch(Event[EventDruh])
	{
		case EVENT_FALLOUT: CreateFallOut();
		case EVENT_PLOSINY: CreatePlosiny();
		case EVENT_DERBY: CreateDerby();
		case EVENT_BLUDISTE: CreateBludiste();
	}

    SetTimerEx("PortToEvent", 2000, 0, "i", playerid);
	return 1;
}

