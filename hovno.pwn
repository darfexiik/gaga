		}

        if(ID < Player2dMaxSeznam[playerid])
		{
			switch(Player2dDialogID[playerid])
			{
				case DIALOG_2D_SKIN:
				{
					PlayerTextDrawSetPreviewModel(playerid, Player2DModel[IDs][playerid], ID);
