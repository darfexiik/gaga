    || (IsPlayerInArea(playerid, -2762.1792, -2675.1492, -514.3894, -413.3973) && pz <= 4.24) //sf südwesten kleiner teich
    || (IsPlayerInArea(playerid, -825.7167, -453.9256, -2072.8215, -1869.9600) && pz <= 5.72) //sf gammel teich
