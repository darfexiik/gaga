        else if(MiseOn[playerid] == 1)          					return SCM(playerid,0xFF0000AA,"Právì pracujete!", 1);
		else if(Sav[VezeniTime][playerid] > 0)     					return SCM(playerid,0xFF0000AA,"Jste ve vìzení!", 1);
