  	    switch(PLAYER_HAZARD[playerid])
  	    {
  	        case 1: DestroyKarty(playerid);
  	        case 2: DestroyAutomat(playerid);
  	        case 3: DestroyKostky(playerid);
  	        case 4: DestroyMince(playerid);
  	    }
  	    if(PLAYER_HAZARD[playerid] > 0) PLAYER_HAZARD[playerid] = 0;

  	    if(Player2dDialogID[playerid] > 0)
	  	{
            if(Player2dDialogID[playerid] == DIALOG_2D_TUNING || Player2dDialogID[playerid] == DIALOG_2D_COLOR)
			{
			    Destroy2dDialog(playerid);
			    ShowTuning(playerid);
			}else Destroy2dDialog(playerid);

			if(Player2dDialogID[playerid] == DIALOG_2D_SKIN)
			{
				ClearAnimEx(playerid);
			}
		}
  	}
  	return 1;
}

