CMD:reloadvip(playerid)
{
    new STR[30], VIPName[MAX_PLAYER_NAME], Nasel;
	if(fexist("Vip.txt"))
	{
		new File:V_FILE = fopen("Vip.txt",io_readwrite);
		while(fread(V_FILE,STR))
		{
  			strdel(STR,NajdiEnter(STR),strlen(STR));
			format(VIPName, MAX_PLAYER_NAME, STR);
			if(!strcmp(VIPName,PlayerName(playerid),true))
 			{
				if(HaveVIP(playerid) == 1)
				{
					Sav[VIP][playerid] += Days(60);
				}else{
				    Sav[VIP][playerid] = 0;
					Sav[VIP][playerid] = gettime() + Days(60);
				}
				DeleteLineFromText("Vip.txt",VIPName);
				Nasel = 1;
			}
		}
		fclose(V_FILE);
	}

	if(Nasel == 1)
	{
		format(MAX_STR,sizeof(MAX_STR),"");
	    format(MAX_STR,sizeof(MAX_STR),"Dìkujeme za zakoupení VIP úètu a podpoøení našeho serveru.\nVáš vip úèet expiruje %s", BanDate(Sav[VIP][playerid], 1));

	    ShowPlayerDialogEx(playerid, INFORMACNI_DIALOG, DIALOG_STYLE_MSGBOX, "VIP - SMS", MAX_STR, "Zavøít", "");

		Log_Write(128,playerid,"Hráè %s si koupil VIP, EXPIRACE: %S",PlayerName(playerid), BanDate(Sav[VIP][playerid], 1));
		UlozitData(playerid);
	}else{
		SCM(playerid,0xFF0000AA,"Váš VIP úèet nebyl nalezen!");
		SCM(playerid,0xFF0000AA,"Jestli jste poslali SMS pøed chvilkou tak mìjte strpení nebo kontaktujte hlavního administrátora.");
	}
	return 1;
}

