public OnLookupResponse(playerid, response, data[])//hostname - 129
{
	if(!IsPlayerConnected(playerid)) return 1;
	if(response != 200)
	{
	    CallLocalFunction("OnLookupComplete", "iii", playerid, 0, 0);
	}
 	else
	{
		new hostname[129], countryCode[17], countryName[65], region[129], asn[129], proxy;

		if(!sscanf(data, "s[128]s[16]s[64]s[128]s[128]i", hostname, countryCode, countryName, region, asn, proxy))
		{
	//Detect DIP
			if(!strcmp(hostname, "dynamic", true, 7))
		 	{
				SCMAT(128, 0xAFAFAFAA, "%s: Dynamic IP: Ano", PlayerName(playerid));
		    }
		    else
		    {
		        SCMAT(128, 0xAFAFAFAA, "%s: Dynamic IP: NE", PlayerName(playerid));
			}
	//Detect DIP


			strmid(lol2[playerid], countryCode, 0, strlen(countryCode));
			strmid(lol3[playerid], countryName, 0, strlen(countryName));

		    if(strfind(asn, "AVAST_Software_a.s.", true) != -1) proxy = 1;
			CheckCountry(playerid);
			if(proxy)
   			{
   			    if(antiproxy == 1)
   			    {
	        		SCMTAF(128, 0xAFAFAFAA, "Hráè %s byl vyhozen za VPN.", PlayerName(playerid));
					SetTimerEx("Kicked",0,0,"i",playerid);
					Kick(playerid);
				}
   			}
		}
	}
	return 1;
}

stock CheckCountry(playerid)//Only CZ,SK,SVK
{
    new allowednames[][] = {"Czech Republic","Slovakia","Slovenia","Czech","Slovak","Czech_Republic"};
	new allowedcountries[][] = {"CZ","SK","SVK"},bool:canplay;
	for(new i; i < sizeof(allowedcountries); i ++)
	{
		if(strcmp(lol2[playerid],allowedcountries[i],true) == 0)
		{
			canplay = true;
			break;
		}
		else
		{
			for(new o; o < sizeof(allowednames); o ++)
			{
				if(strcmp(lol3[playerid],allowednames[o],true) == 0)
				{
					canplay = true;
					break;
				}
			}
		}
	}
	if(canplay == false)
	{
		SCMTAF(128, 0xFF0000AA, "Hráè {007700}%s {ffffff}byl vyhozen z dùvodu: CZ/SK Server.", PlayerName(playerid));
		SetTimerEx("Kicked",0,0,"i",playerid);
	}
}

CMD:vipcar(playerid,params[])
{
    if(Sav[VezeniTime][playerid] > 0)          							return SCM(playerid, -1, "Ve vìznici nemùžete spawnout V.I.P Vozidlo!");
    else if(PlayerInDM[playerid] == 1 && PlayerBox[playerid] == 1)      return SCM(playerid, -1, "V minihøe nelze spawnout V.I.P Vozidlo!!");
    else if(PlayerStroj[playerid] != G_NOTHING) 						return SCM(playerid, -1,"Máte nìco na práci!");
	else if(PlayerInDM[playerid] == 1)                                  return SCM(playerid, -1,"Máte nìco na práci!");
	else if(PlayerInZbrojak[playerid] == 1)                             return SCM(playerid, -1,"Máte nìco na práci!");

    if(HaveVIP(playerid) == 0) return SPIB(playerid, "~r~Nemas V.I.P Tarif!", 5);
    else if(vozitko[playerid])return SendClientMessage(playerid,0xFF0000,"{FF0000}Již máš vytvoøený vipcar, pro znièení {FFFFFF}/vipd.");
    new Float:x,Float:y,Float:z,Float:r;
    GetPlayerPos(playerid,x,y,z);
    GetPlayerFacingAngle(playerid,r);
    vozitko[playerid] = CreateVehicle(541, x,y,z, r+90, 0, 0,60000);
    Car_Gas[GetPlayerVehicleID(playerid)] = 100;
    PutPlayerInVehicle(playerid, vozitko[playerid],0);
    SPIB(playerid, "~w~Spawnul jsis ~r~VIP Vozidlo", 5);
    return 1;
}

CMD:vipd(playerid,params[])
{
    if(Sav[VezeniTime][playerid] > 0)          							return SCM(playerid, -1, "Ve vìznici nemùžete spawnout V.I.P Vozidlo!");
    else if(PlayerInDM[playerid] == 1 && PlayerBox[playerid] == 1)      return SCM(playerid, -1, "V minihøe nelze spawnout V.I.P Vozidlo!!");
    else if(PlayerStroj[playerid] != G_NOTHING) 						return SCM(playerid, -1,"Máte nìco na práci!");
	else if(PlayerInDM[playerid] == 1)                                  return SCM(playerid, -1,"Máte nìco na práci!");
	else if(PlayerInZbrojak[playerid] == 1)                             return SCM(playerid, -1,"Máte nìco na práci!");

    if(HaveVIP(playerid) == 0) return SPIB(playerid, "~r~Nemas V.I.P Tarif!", 5);
    if(!vozitko[playerid])return SPIB(playerid, "~r~Nemas vytvoreny vipcar", 5);
    DestroyVehicle(vozitko[playerid]);
    vozitko[playerid] = 0;
    SPIB(playerid, "~w~Znicil jste ~r~VIP ~w~vozidlo", 5);
    return 1;
}

