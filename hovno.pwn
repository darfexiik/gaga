	    if(Nasobek > 23) Nasobek = 23;
	}
	Vysledek = Zakl_VYplata + (Pripocteni * Nasobek);
	if(Vysledek > CharitaMoney)
	{
	    Odmena(playerid, "~r~V kase nejsou zadne penize ~n~~w~/kasainfo", 5);
	}
	else
	{
		if(HaveJobSkin(playerid) == 0 && HaveVIP(playerid) == 0)
		{
		    SPIB(playerid, "Kdyz budete mit oblecenou uniformu povolani, budete dostavat odmenu v plne vyssi!", 3);
		    SPIBOX[0][playerid] = SetTimerEx("SPIB_Hide", 3*1000, 0, "ii", playerid, 0);

		    Vysledek = Vysledek / 2;
		}
		if(HaveVIP(playerid) == 1)
		{
		    new procen = (Vysledek / 100) * 50;
			Vysledek += procen;
		}

	    Sav[Vyplata][playerid] += Vysledek;

	    ODMENAF(100,playerid,5,"Odmena~n~~w~~h~%s$",Number(Vysledek));

	    new JOBID;
	    for(new i; i < JOBCount; i ++)
		{
			if(JOB[i][JOB_ID] == Hrac[Zamestnani][playerid])
			{
			    JOBID = i;
			}
	 	}

	    Log_Write(100,playerid,"(MISE) %s - Odmìna: %s$ - 1 zk", JOB[JOBID][JOB_Name], Number(Vysledek));

	    CHM_Minus(Vysledek);

	    PlayerPlaySound(playerid,1058,0.0,0.0,0.0);
	}
	AchPovolani(playerid);
    //SCMF(127,playerid,0xFF0000AA,"%d zk Odmìna: %s$",Sav_Body[Zamest][playerid], Number(Vysledek));
	return 1;
}
