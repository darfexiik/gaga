    Set[S_ChatEx][playerid]         	= 1;
    Set[S_KillList][playerid]           = 1;
	//-----------------------------------
	for(new i; i < NastavChat(-1, -1); i ++) NastavChatEx(playerid, i, DefaultSet[i]);
