stock CHeckVehicleFuel()
{
    for(new car = 0; car < MAX_VEHICLE_EX; car ++)
	{
		if(Car_Gas[car] <= 0) return car;
	}
	return 0;
}
stock CHeckVehicleNearFuel(playerid)
{
	new VEHID = -1, Float: dist = 9999.9;
    for(new i = 0; i < MAX_VEHICLE_EX; i ++)
	{
		if(Car_Gas[i] == 0)
		{
			new Float:VehDist = PlayerDistaceToVehicle(playerid, i);
			if(VehDist < dist)
			{
			    new Float:VehX, Float:VehY, Float:VehZ;
			    GetVehiclePos(i, VehX, VehY, VehZ);
			    if(IsPointInCity(VehX, VehY) == IsPlayerInCityRace(playerid))
			    {
			    	dist = VehDist;
			    	VEHID = i;
				}
			}
		}
	}
	return VEHID;
}
