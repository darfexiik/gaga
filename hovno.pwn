stock GetPlayerEnter(playerid)
{
	for(new e;e< EnterCount + 1; e++)
	{
		if(IsPlayerInSphere(playerid, EnterInfo[e][p_X], EnterInfo[e][p_Y], EnterInfo[e][p_Z],3))
		{
			return e;
		}
	}
	return -1;
}
stock GetPlayerExit(playerid)
{
	for(new e;e< EnterCount + 1; e++)
	{
		if(IsPlayerInSphere(playerid, EnterInfo[e][i_X], EnterInfo[e][i_Y], EnterInfo[e][i_Z],3))
		{
		    if(GetPlayerVirtualWorld(playerid) == e)
		    {
				return e;
			}
		}
	}
	return -1;
}
