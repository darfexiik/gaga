stock HaveJobSkin(playerid)
{
	new JObID = Hrac[Zamestnani][playerid];
	switch(JObID)
	{
		case POLDA:
	 	{
	  		switch(GetPlayerSkin(playerid))
	    	{
	     		case 283, 281, 286, 280, 282, 284, 285, 288, 265, 266, 267: return 1;
			}
	    }
		case HASIC:
		{
	 		switch(GetPlayerSkin(playerid))
	   		{
	     		case 277, 278, 279:return 1;
       		}
		}
		case ZDRAVOTNIK:
		{
  			switch(GetPlayerSkin(playerid))
     		{
       			case 274, 275, 276, 70: return 1;
		   	}
		}
		case HOTEL_SLUZBA: if(GetPlayerSkin(playerid) == 185) return 1;
		case STROJVUDCE: if(GetPlayerSkin(playerid) == 255) return 1;
  		default:
  		{
  		    new JOBID;
		    for(new i; i < JOBCount; i ++)
			{
				if(JOB[i][JOB_ID] == Hrac[Zamestnani][playerid])
				{
				    JOBID = i;
				}
		 	}
  		    if(JOB[JOBID][JOB_Oblek] == GetPlayerSkin(playerid)) return 1;
  		}
	}
	return 0;
}
