stock IsPlayerInTop(playerid, RaceID)
{
    for(new i; i < MAX_RACE_TOP; i ++)
	{
	    if(strcmp(PlayerName(playerid),RaceTopName[RaceID][i], false) == 0) return i;
	}
	return -1;
}
