public GYM_CHECK(playerid)
{
    if(PlayerZveda[playerid] == 0)	PlayerSila[playerid] -= 4;
    else                            PlayerSila[playerid] -= 10;
    //--------------------------------------------------------------------------
    switch(PlayerStroj[playerid])
    {
    	case G_BIKE:
    	{
    	    switch(PlayerSila[playerid])
    	    {
    	        case 0 .. 10: 	ApplyAnimationEx(playerid, "GYMNASIUM", "GYM_BIKE_STILL", 4.0, 0, 0, 0, 1, 500, 1);
			    case 11 .. 70: 	ApplyAnimationEx(playerid, "GYMNASIUM", "GYM_BIKE_SLOW",  4.0, 1, 0, 0, 1, 0, 1);
			    default: 		ApplyAnimationEx(playerid, "GYMNASIUM", "GYM_BIKE_FAST",  4.0, 1, 0, 0, 1, 0, 1);
    	    }
    	    if(PlayerSila[playerid] > 90)
    	    {
    	    	if(Sav[PruvodceUkol][playerid] == P_GYM_KOLO) Pruvodce[P_GYM_KOLO][playerid] = 1;
			}
			SetProgressBarValue(Bar[playerid], PlayerSila[playerid]);
			UpdateProgressBar(Bar[playerid], playerid);
    	}
    	case G_PAS:
    	{
    	    switch(PlayerSila[playerid])
    	    {
    	        case 0 .. 20:   ApplyAnimationEx(playerid, "GYMNASIUM", "GYM_TREAD_WALK", 	4.0, 1, 0, 0, 1, 0, 1);
    	        case 21 .. 70:  ApplyAnimationEx(playerid, "GYMNASIUM", "GYM_TREAD_JOG", 	4.0, 1, 0, 0, 1, 0, 1);
    	        default:        ApplyAnimationEx(playerid, "GYMNASIUM", "GYM_TREAD_SPRINT", 4.0, 1, 0, 0, 1, 0, 1);
    	    }
    	    SetProgressBarValue(Bar[playerid], PlayerSila[playerid]);
			UpdateProgressBar(Bar[playerid], playerid);
    	}
    	case G_BENCH:
    	{
    	    if(PlayerSila[playerid] >= 90 && PlayerZveda[playerid] == 0)
    	    {
    	        ApplyAnimationEx(playerid, "BENCHPRESS", "GYM_BP_UP_SMOOTH", 		 1.0, 0, 0, 0, 1, 0, 1);
    	        ApplyAnimation(playerid, "BENCHPRESS", "GYM_BP_UP_SMOOTH", 		 1.0, 0, 0, 0, 1, 0, 1);
    	        PlayerZveda[playerid] = 1;
    	        SetTimerEx("GYM_DOWN", 1800, 0, "ii", playerid, 0);
    	    }
    	    SetProgressBarValue(Bar[playerid], PlayerSila[playerid]);
			UpdateProgressBar(Bar[playerid], playerid);

    	    if(PlayerZveda[playerid] == 0 && PlayerKonec[playerid] == 1) return FallDownGYM(playerid);//chtìl skonèit ale ještì mìl nahoøe èinku
    	}
    	case G_CINKY:
    	{
    	    if(PlayerSila[playerid] >= 90 && PlayerZveda[playerid] == 0)
    	    {
    	    	ApplyAnimationEx(playerid, "FREEWEIGHTS", "GYM_FREE_UP_SMOOTH", 1.0, 0, 0, 0, 1, 0, 1);
    	    	ApplyAnimation(playerid, "FREEWEIGHTS", "GYM_FREE_UP_SMOOTH", 1.0, 0, 0, 0, 1, 0, 1);
                PlayerZveda[playerid] = 1;
    	        SetTimerEx("GYM_DOWN", 1300, 0, "ii", playerid, 0);
    	    }
    	    SetProgressBarValue(Bar[playerid], PlayerSila[playerid]);
			UpdateProgressBar(Bar[playerid], playerid);

    	    if(PlayerZveda[playerid] == 0 && PlayerKonec[playerid] == 1) return FallDownGYM(playerid);//chtìl skonèit ale ještì mìl nahoøe èinky
		}
    }
    //--------------------------------------------------------------------------
    if(PlayerSila[playerid] > 0)
	{
		SetTimerEx("GYM_CHECK", 500, 0, "i", playerid);
    }else{
        if(PlayerStroj[playerid] != G_BENCH)
        {
            if(PlayerStroj[playerid] == G_CINKY)
		    {
      			SetTimerEx("GYM_CHECK", 500, 0, "i", playerid);
		    	PlayerSila[playerid] = 0;
		    }else FallDownGYM(playerid, 1);
		}else{
		    SetTimerEx("GYM_CHECK", 500, 0, "i", playerid);
		    PlayerSila[playerid] = 0;
		}
	}
	return 1;
}

