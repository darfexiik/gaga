public OnVehicleSpawn(vehicleid)
{
    if(PrivesKamion == vehicleid) SetTimerEx("DestroyVehicleEx", 1000,0,"i",PrivesKamion);
    if(PrivesKamionLV == vehicleid) SetTimerEx("DestroyVehicleEx", 1000,0,"i",PrivesKamionLV);
    if(PrivesKamionSF == vehicleid) SetTimerEx("DestroyVehicleEx", 1000,0,"i",PrivesKamionSF);

    GetVehiclePos(vehicleid, VehiclePosX[vehicleid], VehiclePosY[vehicleid], VehiclePosZ[vehicleid]);
    //--------------------------------------------------------------------------
    DestroyObject(Neon2[vehicleid]);
	DestroyObject(Neon[vehicleid]);
	DestroyObject(VehText[vehicleid]);
	DestroyObject(VehText2[vehicleid]);
	VozidloNeon[vehicleid] = 0;
    //--------------------------------------------------------------------------
    if(GetVehicleType(vehicleid) == TYPE_LETADLA || GetVehicleType(vehicleid) == TYPE_BIKES || GetVehicleType(vehicleid) == TYPE_TRAIN || GetVehicleType(vehicleid) == TYPE_BOATS || GetVehicleType(vehicleid) == TYPE_RC)
	{
	    if(Car_Gas[vehicleid] <= 5)
	    {
	    	Car_Gas[vehicleid]  = 50;
		}
	}
	//--------------------------------------------------------------------------
	for(new i = 1; i < HouseCount + 1; i ++)
    {
        for(new x; x < HouseCarCount[i]; x ++)
        {
            if(vehicleid == Veh_ID[x][i])
        	{
         		CarHouseCreate(i, x, 3);
			}
		}
	}
	//--------------------------------------------------------------------------
	for(new i; i < MafieCount; i ++)
    {
        for(new x; x < MafieCarCount[i]; x ++)
        {
            if(vehicleid == MafVeh_ID[x][i])
        	{
         		CarMafieCreate(i, x, 3);
			}
		}
	}
	//--------------------------------------------------------------------------
	new Dodavka_ID = IsVehicleDodavka(vehicleid);
	if(Dodavka_ID != -1)
	{
	    RefrestDodavka(Dodavka_ID);
	    Dodavky[Dodavka_ID][DodavkaActive] = 0;
	}
	//--------------------------------------------------------------------------
	new engine,lights,alarm,doors,bonnet,boot,objective;
	if(Car_Gas[vehicleid] < 1)
 	{
  		GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
	    SetVehicleParamsEx(vehicleid,0,lights,alarm,0,0,0,objective);
 	}else{
 	    GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
		SetVehicleParamsEx(vehicleid,1,lights,alarm,0,0,0,objective);
 	}
 	ZamekCar[vehicleid] = 0;
 	ForMaxPlayer(i) SetVehicleParamsForPlayer(vehicleid, i, 0, 0);
 	//--------------------------------------------------------------------------
 	ForMaxPlayer(i)
 	{
 		if(OdtahVehicle[i] == vehicleid && MiseOn[i] == 1)
 		{
 		    ZacatekMise[i] = 0;
 		    OdtahVehicle[i] = -1;
   			MiseOn[i] = 0;
   			OdtahovakaMise(i);
 		}
	}
	return 1;
}

public OnVehicleDeath(vehicleid, killerid)
{
    GetVehiclePos(vehicleid, VehiclePosX[vehicleid], VehiclePosY[vehicleid], VehiclePosZ[vehicleid]);
    DestroyObject(Neon2[vehicleid]);
	DestroyObject(Neon[vehicleid]);
	DestroyObject(VehText[vehicleid]);
	DestroyObject(VehText2[vehicleid]);
	VozidloNeon[vehicleid] = 0;

    if(IsVehicleInAnyHouse(vehicleid) == 0 && IsVehicleInAnyMafie(vehicleid) == 0 && VehicleCanPark[vehicleid] == 1)
    {
		DestroyVehicleExX(vehicleid);
    }
	return 1;
}

