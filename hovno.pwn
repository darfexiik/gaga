  			if(GetPlayerMoneyEx(playerid) < Race[ra][RACE_Cost]) return SCM(playerid,-1, "{ff0000}[ Server ]{ffffff} Bohužel nemáš dostatek penìz !");
     		GivePlayerMoneyEx(playerid, -Race[ra][RACE_Cost]);
		}else{
  			new stri[127], MafVyziv = Race[ra][NascarIDVyzivatel], MafPrijem = Race[ra][NascarIDPrijemce];
			format(stri, sizeof(stri), "Do tohoto závodu se mùžou pøipojit jen hráèi z mafie %s a %s", Mafie[MafVyziv][MafieName], Mafie[MafPrijem][MafieName]);

			if(Race[ra][NascarIDVyzivatel] != Mafie_ID && Race[ra][NascarIDPrijemce] != Mafie_ID) return SCM(playerid, 0xFF0000AA, stri);
   			SetPlayerColor(playerid, Mafie[Mafie_ID][MafieBarva]);
