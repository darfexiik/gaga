CMD:da(playerid,params[])
{
    if(IsPlayerAdmin(playerid) || Sav[Admin][playerid] > 0 || Sav[Akademie][playerid] == 1)
    {
	    new duvod[40];
	    if(strlen(params) > 40) 			return SCM(playerid,0xFF0000FF, "Nápovìda: 1 - 40 znakù");
	    else if(sscanf(params,"z",duvod))	return SCM(playerid, 0xFF0000FF, "Nápovìda: /da (DÙVOD)");

	    ForMaxPlayer(i) Hlasoval[i] = 0;
		TextDrawHideForAll(Anketa);
		ano = 0;
		ne = 0;
		AnketaON = 0;
		AnketaID = -1;
		KillTimer(AnketaTimer);

        if(Sav[Admin][playerid] == 0 && Sav[Akademie][playerid] == 1)
        {
            SCMTAF(120, 0xFF0000AA,"Akademik %s smazal anketu z dùvodu: %s",PlayerName(playerid),duvod);
		} else SCMTAF(120,0xFF0000AA,"Administrátor %s smazal anketu z dùvodu: %s",PlayerName(playerid),duvod);
	} else SCM(playerid,0xFFFFFFAA,"SERVER: Pøíkaz neexistuje -> /help nebo /prikazy");
	return 1;
}

CMD:anketa(playerid,params[])
{
	new Text[150];
    if(Sav[Last_Job][playerid] != NOVINAR)						return SCM(playerid,0xff0000AA,"Nejste zamìstnán jako novináø");
    else if(sscanf(params,"s", Text))							return SCM(playerid,0xff0000AA,"Nápovìda: /anketa (TEXT)");
	else if(AnketaON == 1)										return SCM(playerid,0xff0000AA,"Anketa právì probíhá");
    else if(strlen(params) < 1 || strlen(params) > 110) 		return SCM(playerid,0xff0000AA,"Nápovìda: /anketa (TEXT) (Max. 100 znakù)");

    for(new i=0; i<strlen(params); i++)
    {
		if(strfind(params[i], "~", true) == 0) return SCM(playerid,0xff0000AA,"V anketì je zakázaný znak '~'.");
	}

	AnketaON = 1;
    AnketaID = playerid;
	format(AnketaText, sizeof(AnketaText), "%s", TextHack(Text));

    new str[200];
	format(str, sizeof(str), "Novinar ~g~%s(%d)~w~ podal anketu:~n~%s~n~~g~/ano (%d) ~r~/ne (%d)",PlayerName(AnketaID),AnketaID,AnketaText,ano,ne);
	TextDrawSetString(Anketa,str);
	TextDrawShowForAll(Anketa);

	AnketaTimer = SetTimer("AnketaHide",60000,0);
	return 1;
}

CMD:aanketa(playerid,params[])
{
	if(IsPlayerAdmin(playerid) || Sav[Admin][playerid] > 1)
