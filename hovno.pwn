CMD:report(playerid, params[])
{
    new id, STR[80];
    if(sscanf(params,"i",id)) 									return SCM(playerid, 0xFF0000AA, "Nápovìda: /report (ID)");
	else if(!IsPlayerConnected(id)) 							return SCM(playerid, 0xFF0000AA, "Zadané ID není pøipojeno");
	else if(IsPlayerNPC(id)) 									return SCM(playerid, 0xFF0000AA, "Nemùžete nahlásit NPC!");
	else if(playerid == id)                                     return SCM(playerid, 0xFF0000AA, "Opravdu porušuješ pravidla?");
    if(Sav[MuteOn][playerid] > 0)
    {
	    new Minuta,Sekunda;
		SecondToMinute(Sav[MuteOn][playerid],Minuta,Sekunda);
		SCMF(127,playerid,0xFF0000AA,"Jste umlèen na %02d:%02d minut",Minuta,Sekunda);
		return 0;
	}

    SetPVarInt(playerid, "ReportID", id);
    format(STR, sizeof(STR), "Nahlásit %s (%d)", PlayerName(id), id);
    ShowPlayerDialogEx(playerid, DIALOG_REPORT, DIALOG_STYLE_LIST, STR, "Vlastní dùvod\nNarušení mise\nNevhodný slovník\nSPAM\nUrážky rodiny\nUrážka administrátora\nNapodobenina administrátora/dlouhodobého hráèe\nReklama (Na jiný SAMP server)\nNarušení portu\nNevhodný nick-name\nUrážka serveru", "Vybrat", "Zavøít");
	return 1;
}
