stock Metry(stri)
{
	new string[50],metr,km;
    MeterToKM(stri,km,metr);
    format(string, sizeof(string), "%d.%d km", km, metr);
	return string;
}

stock MeterToKM(meter,&km,&m)
{
	km = floatround(meter/1000,floatround_floor);
 	m = meter-(km*1000);
}
