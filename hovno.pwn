	else if(SeskokPlayer[playerid] == 1)						return SCM(playerid,0xFF0000AA,"Již jste pøihlášen v seskoku!", 1);
	else if(PlayerBox[playerid] == 1)							return SCM(playerid,0xFF0000AA,"Již jste pøihlášen v boxu!", 1);
    else if(PlayerStroj[playerid] != G_NOTHING) 				return SCM(playerid,0xff0000AA,"Máte nìco na práci!");
    else if(PlayerInZbrojak[playerid] == 1)                     return SCM(playerid,0xff0000AA,"Máte nìco na práci!");

	if(!sscanf(params,"s",race))
	{
		//else if(PlayerInDM[playerid] == 1)			return SCM(playerid,0xFF0000AA,"Již si pøihlášen v dm!");
		//**********************************************************************
		new ra = GetRaceName(race);
