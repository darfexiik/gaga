stock RandomFall()
{
	new ID = random(MAX_FALL);
    if(FallObjectActive[ID] == 1) return ID;
	return -1;
}

stock ObjectSpadnutych()
{
	new Counttt;
	for(new i; i < MAX_FALL; i ++)
	{
	    if(FallObjectActive[i] == 0) Counttt ++;
	}
	return Counttt;
}

stock FallOutDown()
{
	new ID = RandomFall();
	if(ID == -1) return FallOutDown();

	new Float:oX, Float:oY, Float:oZ;
	GetObjectPos(FallObjectID[ID], oX, oY, oZ);

	FallObjectActive[ID] = 0;
	MoveObject(FallObjectID[ID],oX, oY, oZ + 0.05, FALL_OUT_SPEED, 0.0000, -5.0000, 0.0000);
	return 1;
}

stock CreateFallOut()
{
	new ID, Barva;
	for(new i; i < (MAX_FALL / 10); i ++)
	{
	    for(new x; x < (MAX_FALL / 10); x ++)
		{
		    FallObjectID[ID] = CreateObject(19128, FALL_OUT_X + (FALL_OUT_DIST * i), FALL_OUT_Y + (FALL_OUT_DIST * x), FALL_OUT_Z, 0.0000, 0.0000, 0.0000);

			switch(Barva)
			{
			    case 0: SetObjectMaterialText(FallObjectID[ID], "Any text", 0, OBJECT_MATERIAL_SIZE_256x128, "Arial", 28, 0, 0xFF0000FF, 0xFF0000FF, OBJECT_MATERIAL_TEXT_ALIGN_CENTER);
				case 1: SetObjectMaterialText(FallObjectID[ID], "Any text", 0, OBJECT_MATERIAL_SIZE_256x128, "Arial", 28, 0, 0x0000FFFF, 0x0000FFFF, OBJECT_MATERIAL_TEXT_ALIGN_CENTER);
			}

			FallObjectActive[ID] 	= 1;
            FallObjectStav[ID] 		= 0;
			ID ++;

			if(Barva == 1) 	Barva = 0;
			else            Barva = 1;
		}
		if(Barva == 1) 	Barva = 0;
		else            Barva = 1;
	}
	return 1;
}

stock CreatePlosiny()
{
	for(new i; i < MAX_PLOSINY ; i ++)
	{
    	FallObjectID[i] = CreateObject(19128, FALL_OUT_X, FALL_OUT_Y - (FALL_OUT_DIST * i), FALL_OUT_Z, 0.0000, 0.0000, 0.0000);
    	SetObjectMaterialText(FallObjectID[i], "Any text", 0, OBJECT_MATERIAL_SIZE_256x128, "Arial", 28, 0, 0xFF84E812, 0xFF84E812, OBJECT_MATERIAL_TEXT_ALIGN_CENTER);

        FallObjectActive[i] 	= 1;
		FallObjectStav[i] 		= 0;
	}

	new Barva, ID = 1;
	for(new i = MAX_PLOSINY; i < (MAX_FALL / 4); i ++)
	{
	    if((MAX_FALL / 4) - 1 != i)
	    {
		    switch(Barva)
			{
			    case 0:
			    {
		    		FallObjectID[i] = CreateObject(19128, FALL_OUT_X + PLOSINY_DISTANCE, FALL_OUT_Y + (FALL_OUT_DIST * ID), FALL_OUT_Z, 0.0000, 0.0000, 0.0000);
		    		SetObjectMaterialText(FallObjectID[i], "Any text", 0, OBJECT_MATERIAL_SIZE_256x128, "Arial", 28, 0, 0xFF0000FF, 0xFF0000FF, OBJECT_MATERIAL_TEXT_ALIGN_CENTER);
				}
				case 1:
			    {
		    		FallObjectID[i] = CreateObject(19128, FALL_OUT_X - PLOSINY_DISTANCE, FALL_OUT_Y + (FALL_OUT_DIST * ID), FALL_OUT_Z, 0.0000, 0.0000, 0.0000);
		    		SetObjectMaterialText(FallObjectID[i], "Any text", 0, OBJECT_MATERIAL_SIZE_256x128, "Arial", 28, 0, 0x0000FFFF, 0x0000FFFF, OBJECT_MATERIAL_TEXT_ALIGN_CENTER);
				}
			}

			FallObjectActive[i] 	= 1;
	  		FallObjectStav[i] 		= 0;

			if(Barva == 1) 	Barva = 0;
			else            Barva = 1;
		}else{
		    FallObjectID[i] = CreateObject(19128, FALL_OUT_X, FALL_OUT_Y + (FALL_OUT_DIST * ID), FALL_OUT_Z, 0.0000, 0.0000, 0.0000);
    		SetObjectMaterialText(FallObjectID[i], "Any text", 0, OBJECT_MATERIAL_SIZE_256x128, "Arial", 28, 0, 0xFF84E812, 0xFF84E812, OBJECT_MATERIAL_TEXT_ALIGN_CENTER);

            Event[EventPickup] = CreatePickup(1274, 1, FALL_OUT_X, FALL_OUT_Y + (FALL_OUT_DIST * ID), FALL_OUT_Z + 1, -1);

            Event[EventSpawnDM_X][0] = FALL_OUT_X;
			Event[EventSpawnDM_Y][0] = FALL_OUT_Y + (FALL_OUT_DIST * ID);
			Event[EventSpawnDM_Z][0] = FALL_OUT_Z + 1;

			FallObjectActive[i] 	= 1;
	  		FallObjectStav[i] 		= 0;
		}
		ID ++;
	}
	return 1;
}

