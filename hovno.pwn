stock VratComponent(playerid, IDs)
{
    if(PlayerInTuning[playerid] == 1)
	{
	    if(VehicleTextTuning[playerid] == 0)
	    {
			if(PlayerInstal[playerid] != 0)
			{
				SetTimerEx("TuningENTER", 250, 0, "ii", playerid, IDs);
			}
		}else{
  			switch(VehicleTextTuning[playerid])
	    	{
				case 1:
				{
    				new VehID = GetPlayerVehicleID(playerid);
					if(VehicleText[VehID] == 1)
 					{
						DestroyObject(VehText[VehID]);
		   				DestroyObject(VehText2[VehID]);
		   				VehicleText[VehID] = 0;
