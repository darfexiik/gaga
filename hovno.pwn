stock JoinJOB(playerid, JobID, Skin_J, Slot)
{
    if(PlayerInSkola[playerid] == 1) return 0;

	if(JOB[JobID][JOB_Prukaz] == J_AUTOSKOLA && Sav[Prukaz_Auto][playerid] == 0) 				return SCM(playerid, 0xFF0000FF, "Pro toto povolání potøebujete prùkaz tøídy B (Auto)");
	else if(JOB[JobID][JOB_Prukaz] == J_MOTOSKOLA && Sav[Prukaz_Moto][playerid] == 0) 			return SCM(playerid, 0xFF0000FF, "Pro toto povolání potøebujete prùkaz tøídy A (Motorka)");
	else if(JOB[JobID][JOB_Prukaz] == J_LODNI_SKOLA && Sav[Prukaz_Lod][playerid] == 0) 			return SCM(playerid, 0xFF0000FF, "Pro toto povolání potøebujete prùkaz Lodní prùkaz");
	else if(JOB[JobID][JOB_Prukaz] == J_LETECKA_SKOLA && Sav[Prukaz_Letadlo][playerid] == 0) 	return SCM(playerid, 0xFF0000FF, "Pro toto povolání potøebujete prùkaz Letecký prùkaz");
