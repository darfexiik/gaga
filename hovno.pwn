{
    if(Event[EventVytvoren] == 1 && Event[EventDruh] == EVENT_FALLOUT)
    {
        FallOutDown();

        new Member, Float:GetZ;
	    ForMaxPlayer(i)
	    {
	        if(PlayerEvent[i] == 1)
			{
			    GetPlayerPos(i, GetZ, GetZ, GetZ);
			    if(GetZ >= FALL_OUT_Z - 1)
			    {
					Member ++;
				}else{
				    BackToport(i, 1);
				    PlayerEvent[i] = 0;
				}
			}
	    }

        /*if(ObjectSpadnutych() > 60)
        {
	        for(new i; i < MAX_FALL; i ++)
	        {//poiuz
	            //printf("objID %d lidí na objectu %d lidí v eventu %d", i, MemberInObject(i), Member);
	        	if(MemberInObject(i) == Member)
	        	{
	        	    new VitezPenize = (Event[EventVyhra] / Member);
	        	    ForMaxPlayer(x)
				    {
				        if(PlayerEvent[x] == 1)
						{
						    BackToport(x, 1);
						    GetPlayerMoney(x, VitezPenize);
						    PlayerEvent[x] = 0;
	         				SCMTAF(120, 0xFF0000FF, "%s vyhrál event %s o %s$", PlayerName(x), Event[EventName], Number(VitezPenize));
                            ODMENAF(100,x,5,"Odmena~n~~g~~h~%s$",Number(VitezPenize));
						}
					}
					ZrusEvent();
	        	}
			}
		}*/

		if(Member == 0)
		{
		    SCMTAF(120, 0xFF0000FF, "Event %s nikdo nevyhrál", Event[EventName]);
		    ZrusEvent();
		}

		if(Member == 1)
		{
		    new ID = GetEventWinner();
      		if(ID != -1)
      		{
            	SCMTAF(120, 0xFF0000FF,"%s vyhrál event %s o %s$", PlayerName(ID), Event[EventName], Number(Event[EventVyhra]));
             	ODMENAF(100,ID,5,"Odmena~n~~w~~h~%s$",Number(Event[EventVyhra]));
            	GivePlayerMoneyEx(ID, Event[EventVyhra]);
            	Log_Write(100,ID,"(EVENT) Vyhrál %s za %s$", EventDruhName(Event[EventDruh]), Number(Event[EventVyhra]));
                CHM_Minus(Event[EventVyhra]);
				SetTimerEx("EventEnd", 2000, 0, "i", ID);
			}
			ZrusEvent();
		}
        SetTimer("CHECK_FallOUT", 2000, 0);
    }
	return 1;
