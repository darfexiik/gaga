stock NpcSpawnID(ID)
{
	if(	strcmp(PlayerName(ID), NPC_PIMP_LV, true) == 0	||
	    strcmp(PlayerName(ID), NPC_PIMP_LS, true) == 0	||
	    strcmp(PlayerName(ID), NPC_PIMP_SF, true) == 0 ) return 1;//kvuli pilotum

	if(strcmp(PlayerName(ID), NPC_PRODEJ,false) == 0)return 2;


	if(	strcmp(PlayerName(ID), NPC_BORROW_LV, true) == 0	||
	    strcmp(PlayerName(ID), NPC_BORROW_SF, true) == 0 ) return 4;

	if(strcmp(PlayerName(ID), NPC_DEATH_LS, true) != -1) return 5;
	if(strcmp(PlayerName(ID), "testnpc", true) != -1) return 6;
	return -1;
}
