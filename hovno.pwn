		SetVehicleVirtualWorld(Race[ra][RACE_Cars][playerid], Race[ra][RACE_VW]);
		LinkVehicleToInterior(Race[ra][RACE_Cars][playerid], 0);
		PutPlayerInVehicleEx(playerid, Race[ra][RACE_Cars][playerid], 0);
		TogglePlayerControllable(playerid, 0);
		SetPlayerArmedWeapon(playerid,0);
		//**********************************************************************
		ForMaxPlayer(i)
		{
			switch(Set[S_Zavody][i])
			{
				case 1: SCMTA(playerid, 0x194721FF,"Hráè %s(%d) se pøipojil do závodu {fca800}%s",PlayerName(playerid),playerid), Race[ra][RACE_NAME]);
				case 2: SCMTA(playerid, 0x194721FF,"Hráè %s(%d) se pøipojil do závodu {fca800}%s",PlayerName(playerid),playerid), Race[ra][RACE_NAME]);
    		}
		}

		Race[ra][RACE_PocetR] ++;
		if(Race[ra][Race_Or_Gr] == 0) SPIB(playerid, "LMB pro oznameni pripravenosti v zavode.", 5);

		Log_Write(100,playerid,"Hráè %s se pøipojil do závodu %s", PlayerName(playerid), Race[ra][RACE_NAME]);
		return 1;
	}else{
	    new STR[500],ID;
	    for(new ra=1; ra< RaceCount+1; ra++)
		{
		    if(Race[ra][RACE_Created] == 1 && Race[ra][RACE_Zahajeni] != 0)
		    {
		        new Minuta,Sekunda;
				SecondToMinute(Race[ra][RACE_Zahajeni],Minuta,Sekunda);
		        ID ++;
		        format(STR, sizeof(STR), "%s%d) %s o {84E812}%s$ (%d/8) (%02d:%02d){FFFFFF}\n", STR, ID, Race[ra][RACE_NAME], Number(Race[ra][RACE_Cost]), Race[ra][RACE_PocetR], Minuta,Sekunda);
