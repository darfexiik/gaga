stock UlozMafii(MafieID)
{
    new Data[200], STR[50];
	format(STR, sizeof(STR), "Mafie/Mafian%d.txt", MafieID);
	if(fexist(STR) || !fexist(STR))
	{
		new File:PFile = fopen(STR,io_write);
		format(Data,sizeof(Data),"%s\r\n", Mafie[MafieID][MafieMajitel]);		fwrite(PFile,Data);
		format(Data,sizeof(Data),"%s\r\n", Mafie[MafieID][MafieName]);			fwrite(PFile,Data);
		format(Data,sizeof(Data),"%d\r\n", Mafie[MafieID][MafieKoupena]);		fwrite(PFile,Data);
		format(Data,sizeof(Data),"%d\r\n", Mafie[MafieID][MafieTrezor]);		fwrite(PFile,Data);
		format(Data,sizeof(Data),"%d\r\n", Mafie[MafieID][MafieVlada]);			fwrite(PFile,Data);
		format(Data,sizeof(Data),"%d\r\n", Mafie[MafieID][MafieMAXClenove]);	fwrite(PFile,Data);
		format(Data,sizeof(Data),"%d\r\n", Mafie[MafieID][MafieMAXTrezor]);		fwrite(PFile,Data);
		format(Data,sizeof(Data),"%d\r\n", Mafie[MafieID][MafieMAXAuta]);		fwrite(PFile,Data);
		format(Data,sizeof(Data),"%d\r\n", Mafie[MafieID][MafieMAXDodavky]);	fwrite(PFile,Data);
		format(Data,sizeof(Data),"%d\r\n", Mafie[MafieID][MafieMAXObchody]);	fwrite(PFile,Data);
		//----------------------------------------------------------------------
		for(new p; p < MAX_MAFIE_HODNOST; p ++)
		{
			format(Data,sizeof(Data),"%d\r\n", MafieHodnost[p][MafieID]);		fwrite(PFile,Data);
		}
		//----------------------------------------------------------------------
		for(new p; p < MAX_MAFIE_PLAYER; p ++)
		{
			format(Data,sizeof(Data),"%s %d %d %d %d %d %d %d %d %d %d %d %d %d\r\n", MP[p][MafieID], MPHodnost[p][MafieID], MPVybrat[p][MafieID], MPRespawn[p][MafieID], MPKick[p][MafieID], MPSklad[p][MafieID], MPCar[p][MafieID], MPUpgrad[p][MafieID], MPVyplata[p][MafieID], MPPravomoce[p][MafieID], MPSpawn[p][MafieID], MafieVyplataGIVE[p][MafieID], MPRace[p][MafieID], MPGUN[p][MafieID]);
			fwrite(PFile,Data);
		}
		//----------------------------------------------------------------------
		format(Data,sizeof(Data),"%d\r\n", Mafie[MafieID][MafieBarvaID]);	fwrite(PFile,Data);
		//----------------------------------------------------------------------
		for(new p; p < MAX_MAFIE_ZBRANE; p ++)
		{
			format(Data,sizeof(Data),"%d\r\n", MafieNaboje[p][MafieID]);		fwrite(PFile,Data);
		}
	    fclose(PFile);
	}
	Update3DTextMafie(MafieID);
	CHECK_TOP_MAFIE();

	if(Mafie[MafieID][MafieKoupena] == 1)
	{
		if(Mafie[MafieID][MafieVlada] <= 0) ProdejMafii(MafieID, 1);
	}
	return 1;
}

stock ProdejMafii(MafieID, IDs = 0)
{
    if(strcmp(Mafie[MafieID][MafieMajitel], "INVALID", false) != 0)
    {
	    switch(IDs)
	    {
	        case 1:
			{
			    SCMTAF(128,0xFF0000AA,"Gang %s byl rozpuštìn z nedostatku respektu", Mafie[MafieID][MafieName]);
			    Log_Mafie(128,MafieID,"Gang %s byl rozpuštìn z nedostatku respektu", Mafie[MafieID][MafieName]);
			}
		}

        for(new i; i < ObchodCount; i ++)
		{
		    new MafieIDs = Obchod[i][MafieObchodID];
      		if(MafieIDs != -1)
        	{
        	    if(MafieIDs == MafieID)
				{
					Obchod[i][MafieObchodID] = -1;
                    SaveObchod(i);
				}
			}
		}

        ForMaxPlayer(i)
		{
			if(PlayerMafieID[i] != -1)
			{
   				if(Set[S_Zona][i] == 1) UkazZonu(i);
			}
		}

	    format(Mafie[MafieID][MafieName], MAX_MAFIE_NAME, "INVALID");
	    format(Mafie[MafieID][MafieMajitel], MAX_PLAYER_NAME, "INVALID");

	    for(new i; i <MAX_MAFIE_PLAYER; i ++)
	 	{
	 	    new ID2 = GetPlayerID(MP[i][MafieID]);
			if(IsPlayerConnected(ID2))
			{
				if(GetInteriorMafie(ID2) != -1)
				{
				    MafieExit(ID2, MafieID);
				}

			    UlozPlayerMafii(MP[i][MafieID], MafieID, -1, -1);
			}
	 	}

		for(new i; i <MAX_MAFIE_PLAYER; i ++)
	 	{
	 		format(MP[i][MafieID], MAX_PLAYER_NAME, "INVALID");
	  		MPHodnost[i][MafieID] = -1;
	   		MPVybrat[i][MafieID] = -1;
	    	MPRespawn[i][MafieID] = -1;
	    	MPKick[i][MafieID] = -1;
	    	MPSklad[i][MafieID] = -1;
	    	MPCar[i][MafieID] = -1;
	    	MPUpgrad[i][MafieID] = -1;
	    	MPVyplata[i][MafieID] = -1;
	    	MPPravomoce[i][MafieID] = -1;
	    	MPSpawn[i][MafieID] = -1;
	    	MafieVyplataGIVE[i][MafieID] = 0;
	    	MPRace[i][MafieID] = -1;
	    	MPGUN[i][MafieID] = -1;
	    }

		for(new i; i < MAX_MAFIE_HODNOST; i ++) MafieHodnost[i][MafieID] = 0;
        for(new i; i < MAX_MAFIE_ZBRANE; i ++) MafieNaboje[i][MafieID] = 0;


		CHM_Plus(Mafie[MafieID][MafieMAXTrezor]);
		Mafie[MafieID][MafieKoupena] = 0;
		Mafie[MafieID][MafieTrezor] 	= 0;
		Mafie[MafieID][MafieVlada] = -1;
		Mafie[MafieID][MafieMAXClenove] = 0;
		Mafie[MafieID][MafieMAXTrezor] = 0;
		Mafie[MafieID][MafieMAXAuta] = 0;
		Mafie[MafieID][MafieMAXDodavky] = 0;
		Mafie[MafieID][MafieMAXObchody] = 0;

		for(new ID; ID < MAX_MAFIE_CAR; ID ++)
		{
			if(MafVeh_ID[ID][MafieID] != 0)
			{
			    DestroyVehicleExX(MafVeh_ID[ID][MafieID]);
			    MafVeh_ID[ID][MafieID] 				= 0;
				MafVeh_ID_Druh[ID][MafieID] 		= 0;
				//--
				for(new ComID; ComID < MAX_MAFIE_COMPONENT; ComID ++)
				{
					MafVeh_Component[ComID][ID][MafieID] = 0;
				}
				format(MafVeh_Text[ID][MafieID], 100, "");
				MafVeh_TextColor[ID][MafieID] = -1;
				MafVeh_TextSize[ID][MafieID] = 140;
				UlozMafieCar(MafieID,ID);
			}
		}

		for(new i; i < DodavkyCount; i ++)
		{
  			if(Dodavky[i][MafieOWN_ID] == MafieID)
   			{
      			ChangeVehicleColor(Dodavky[i][DodavkaID], 0, 0);

				Dodavky[i][MafieOWN_ID] = -1;
				Dodavky[i][DodavkaCas] = 0;

                RefrestDodavka(i);
				UlozDodavku(i);
	    	}
		}

		UlozMafii(MafieID);
		CHM_Plus(Mafie[MafieID][MafieTrezor]);
	}
	return 1;
}

stock UlozPlayerMafii(const PlayerNames[], MafieID, MafieIDs = -1, MafieHodnosts = -1, MafieVybrat = -1, MafieRespawn = -1, MafieKick = -1, MafieSklad = -1, MafieCar = -1, MafieUpgrad = -1, MafieVyplata = -1, MafiePrava = -1, MafieSpawn = -1, MafieRace = -1, MafieGUN = -1)
{
	new STR_Name[MAX_PLAYER_NAME], Misto, ID2;

	format(STR_Name, sizeof(STR_Name), "%s", PlayerNames);
	Misto = FingPlayerMisto(MafieID, STR_Name);
	ID2 = GetPlayerID(STR_Name);
	//--------------------------------------------------------------------------
	if(Misto == -1)
	{
	    new ID = FingFreeMisto(MafieID);
	    if(ID != -1)
	    {
	        format(MP[ID][MafieID], MAX_PLAYER_NAME, STR_Name);
			if(MafieHodnosts != -1) 	MPHodnost[ID][MafieID] = MafieHodnosts;
			if(MafieVybrat != -1) 		MPVybrat[ID][MafieID] = MafieVybrat;
