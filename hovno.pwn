stock CreateSeznam2D(playerid)
{
	Player2dMaxSeznam[playerid] = 0;
    switch(Player2dDialogID[playerid])
	{
		case DIALOG_2D_SKIN: Player2dMaxSeznam[playerid] = MAX_2D_SKINS;
		case DIALOG_2D_COLOR: Player2dMaxSeznam[playerid] = MAX_2D_COLORS;
		case DIALOG_2D_SALON:
		{
  			new SalonID = GetPVarInt(playerid,"Salon_ID");
	    	for(new x; x < sizeof(SalonCars); x ++)
			{
				if(SalonID == SalonCars[x][Veh_SaloonID])
				{
				    Player2dMaxSeznam[playerid] ++;
				}
			}
		}
		case DIALOG_2D_BUFET:
		{
		    for(new x; x < sizeof(Bufets); x ++)
			{
			    Player2dMaxSeznam[playerid] ++;
			}
		}
		case DIALOG_2D_AMU:
		{
		    for(new x; x < sizeof(AmuBuy); x ++)
			{
			    Player2dMaxSeznam[playerid] ++;
			}
		}
		case DIALOG_2D_ALKOHOL:
		{
		    for(new x; x < sizeof(AlkoholBuy); x ++)
			{
			    Player2dMaxSeznam[playerid] ++;
			}
		}
		case DIALOG_2D_TUNING:
		{
			for(new i = 1000; i < 1194; i ++)
			{
			    new Model = GetVehicleModel(GetPlayerVehicleID(playerid));
			    if(IsVehicleUpgradeCompatible(Model, i))
			    {
                    Player2dMaxSeznam[playerid] ++;
			    }
			}
		}
	}
	new STR[20], SeznamCount, Float:STRC = (float(Player2dMaxSeznam[playerid]) / 20.0);
	for(new ID; ID < floatround(STRC, floatround_ceil); ID ++)
	{
	    new Vypocet = (SeznamCount + 19);
	    if((Player2dMaxSeznam[playerid] - Vypocet) >= 0) 	format(STR, sizeof(STR), "%d-%d", SeznamCount, SeznamCount + 19);
	    else                                                format(STR, sizeof(STR), "%d-%d", SeznamCount, Player2dMaxSeznam[playerid]);

	    PlayerSeznam2D[ID][playerid] = CreatePlayerTextDraw(playerid,475.0, 155.0 + (10.0 * ID), STR);
		PlayerTextDrawAlignment(playerid,PlayerSeznam2D[ID][playerid], 1);
		PlayerTextDrawBackgroundColor(playerid,PlayerSeznam2D[ID][playerid], 255);
		PlayerTextDrawFont(playerid,PlayerSeznam2D[ID][playerid], 1);
		PlayerTextDrawLetterSize(playerid,PlayerSeznam2D[ID][playerid], 0.160000, 1.100000);
		PlayerTextDrawColor(playerid,PlayerSeznam2D[ID][playerid], -1);
		PlayerTextDrawSetOutline(playerid,PlayerSeznam2D[ID][playerid], 0);
		PlayerTextDrawSetProportional(playerid,PlayerSeznam2D[ID][playerid], 1);
		PlayerTextDrawSetShadow(playerid,PlayerSeznam2D[ID][playerid], 1);
		PlayerTextDrawSetSelectable(playerid,PlayerSeznam2D[ID][playerid], 1);

		PlayerTextDrawUseBox(playerid,PlayerSeznam2D[ID][playerid], 1);
		PlayerTextDrawBoxColor(playerid,PlayerSeznam2D[ID][playerid], 0x00000000);
		PlayerTextDrawTextSize(playerid,PlayerSeznam2D[ID][playerid], 500.000000, 5.000000);
		PlayerTextDrawSetSelectable(playerid,PlayerSeznam2D[ID][playerid], 1);
		PlayerTextDrawShow(playerid, PlayerSeznam2D[ID][playerid]);
		SeznamCount += 20;
	}
	return 1;
}

stock Create2dModels(playerid, CreateOn = 0)
{
	new PlayerXCount, PlayerYCount, Pages = Player2dPages[playerid], STR[120];

	for(new ID = (MAX_MODELS_2D * Pages); ID < (MAX_MODELS_2D * Pages) + MAX_MODELS_2D; ID ++)
