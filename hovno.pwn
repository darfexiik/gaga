							PlayerTextDrawSetString(playerid, Player2DName[IDs][playerid], STR);
							PlayerTextDrawShow(playerid, Player2DName[IDs][playerid]);
						}
					}
				}
				case DIALOG_2D_TUNING:
				{
				    new CCount;
					for(new i = 1000; i < 1194; i ++)
					{
					    new Model = GetVehicleModel(GetPlayerVehicleID(playerid));
					    if(IsVehicleUpgradeCompatible(Model, i))
					    {
					        if(ID == CCount)
				    		{
				    		    switch(GetVehicleComponentType(i))
				    		    {
				    		        case CARMODTYPE_WHEELS: PlayerTextDrawSetPreviewRot(playerid, Player2DModel[IDs][playerid], -10.000000, 0.000000, 90.000000, 1.000000);
									case CARMODTYPE_EXHAUST: PlayerTextDrawSetPreviewRot(playerid, Player2DModel[IDs][playerid], 0.000000, 0.000000, 0.000000, 1.000000);
									case CARMODTYPE_ROOF: PlayerTextDrawSetPreviewRot(playerid, Player2DModel[IDs][playerid], -20.000000, 0.000000, 0.000000, 0.600000);
									case CARMODTYPE_LAMPS: PlayerTextDrawSetPreviewRot(playerid, Player2DModel[IDs][playerid], -20.000000, 0.000000, 0.000000, 1.000000);
									default: PlayerTextDrawSetPreviewRot(playerid, Player2DModel[IDs][playerid], -10.000000, 0.000000, 0.000000, 1.000000);
								}
	      						PlayerTextDrawSetPreviewModel(playerid, Player2DModel[IDs][playerid], i);
