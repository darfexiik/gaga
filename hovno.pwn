public OnPlayerUpdate(playerid)
{
    //--------------------------------------------------------------------------
	if(	IsPlayerInArea(playerid,  1749.8782, 1813.9310, -1728.3934, -1676.0796) ||
		IsPlayerInArea(playerid,  -1994.5334, -1908.3115, 551.3755, 624.5874) 	||
		IsPlayerInArea(playerid,  2072.4409, 2242.1787, 1373.1871, 1482.8796) 	||
		Sav[VezeniTime][playerid] > 0)
	{
  		if(GetPlayerWeapon(playerid) != 0)
   		{
     		if(GetPlayerVirtualWorld(playerid) == 0)SetPlayerArmedWeapon(playerid,0);
		}
	}
	//--------------------------------------------------------------------------
    if (GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
    {
        new Float:vec[3];
        GetPlayerCameraFrontVector(playerid, vec[0], vec[1], vec[2]);
        for (new i = 0; i < sizeof(vec); i++)
            if (floatabs(vec[i]) > 10.0)
                return 0;
    }

	if(PlayerSelectedObject[playerid] == 1)
	{
        if(IsPlayerAdmin(playerid) || Sav[Admin][playerid] > 1)
		{
		    if(PlayerFreezeEdited[playerid] == 1)
		    {
		        new ID = PlayerClickObject[playerid];
		        new Keys,ud,lr;
    			GetPlayerKeys(playerid,Keys,ud,lr);

		        if(Keys & KEY_FIRE)
		        {
			        if(ud == KEY_UP) 	AdminObjectMove(playerid, ID, 0, PlayerSpeedObject[playerid], 0, 0, 0, 0);
			    	if(ud == KEY_DOWN) 	AdminObjectMove(playerid, ID, 0, -PlayerSpeedObject[playerid], 0, 0, 0, 0);
				    if(lr == KEY_LEFT)  AdminObjectMove(playerid, ID, -PlayerSpeedObject[playerid], 0, 0, 0, 0, 0);
				    if(lr == KEY_RIGHT) AdminObjectMove(playerid, ID, PlayerSpeedObject[playerid], 0, 0, 0, 0, 0);
				}
				if(Keys & KEY_WALK)
		        {
			        if(ud == KEY_UP) 	AdminObjectMove(playerid, ID, 0, 0, PlayerSpeedObject[playerid], 0, 0, 0);
			    	if(ud == KEY_DOWN) 	AdminObjectMove(playerid, ID, 0, 0, -PlayerSpeedObject[playerid], 0, 0, 0);
				}
				if(Keys & KEY_SPRINT)
		        {
			        if(ud == KEY_UP) 	AdminObjectMove(playerid, ID, 0, 0, 0, 0, 0, (PlayerSpeedObject[playerid] * 5));
			    	if(ud == KEY_DOWN) 	AdminObjectMove(playerid, ID, 0, 0, 0, 0, 0, -(PlayerSpeedObject[playerid] * 5));
			    	if(lr == KEY_LEFT) 	AdminObjectMove(playerid, ID, 0, 0, 0, -(PlayerSpeedObject[playerid] * 5), 0, 0);
			    	if(lr == KEY_RIGHT) AdminObjectMove(playerid, ID, 0, 0, 0, (PlayerSpeedObject[playerid] * 5), 0, 0);
				}
			}
		}
	}
	Hrac[Aktivita][playerid] = gettime();
	return 1;
}

stock AdminObjectMove(playerid, Object_ID, Float:CisloX, Float:CisloY, Float:CisloZ, Float:rCisloX, Float:rCisloY, Float:rCisloZ)
{
	new Float:ObjectP_X, Float:ObjectP_Y, Float:ObjectP_Z, Float:rObjectP_X, Float:rObjectP_Y, Float:rObjectP_Z;

	new Float:Object_A;

	GetPlayerFacingAngle(playerid, Object_A);
	GetObjectPos(Object_ID, ObjectP_X, ObjectP_Y, ObjectP_Z);
	GetObjectRot(Object_ID, rObjectP_X, rObjectP_Y, rObjectP_Z);

	if(CisloX != 0)
	{
	    new Float:a;
		GetPlayerPos(playerid, a, a, a);
		if (IsPlayerInAnyVehicle(playerid)) GetVehicleZAngle(GetPlayerVehicleID(playerid), a);
		else GetPlayerFacingAngle(playerid, a);
		//----
		ObjectP_X += (CisloX * floatsin(-(a - 90), degrees));
		ObjectP_Y += (CisloX * floatcos(-(a - 90), degrees));
	}
	if(CisloY != 0)
	{
		new Float:a;
		GetPlayerPos(playerid, a, a, a);
		if (IsPlayerInAnyVehicle(playerid)) GetVehicleZAngle(GetPlayerVehicleID(playerid), a);
		else GetPlayerFacingAngle(playerid, a);
		//----
		ObjectP_X += (CisloY * floatsin(-a, degrees));
		ObjectP_Y += (CisloY * floatcos(-a, degrees));
	}
	MoveObject(Object_ID, ObjectP_X, ObjectP_Y, ObjectP_Z + CisloZ, 2, rObjectP_X + rCisloX, rObjectP_Y + rCisloY, rObjectP_Z + rCisloZ);
	return 1;
}

