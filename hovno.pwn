stock GetUpgrades(Upgrade, UpNow, &UP_ID, &UP_CENA)
{
	switch(Upgrade)
	{
	    case 0:
	    {
	        if(UpNow == 5) {UP_ID = 10; UP_CENA = 5000000;}
	        if(UpNow == 10) {UP_ID = 15; UP_CENA = 10000000;}
	        if(UpNow == 15) {UP_ID = -1; UP_CENA = -1;}
		}
		case 1:
	    {
	        if(UpNow == 4) {UP_ID = 6; UP_CENA = 5000000;}
	        if(UpNow == 6) {UP_ID = 8; UP_CENA = 15000000;}
	        if(UpNow == 8) {UP_ID = -1; UP_CENA = -1;}
		}
		case 2:
	    {
	        if(UpNow == 1) {UP_ID = 2; UP_CENA = 5000000;}
	        if(UpNow == 2) {UP_ID = 3; UP_CENA = 10000000;}
	        if(UpNow == 3) {UP_ID = 4; UP_CENA = 15000000;}
	        if(UpNow == 4) {UP_ID = -1; UP_CENA = -1;}
		}
		case 3:
	    {
	        if(UpNow == 10000000) {UP_ID = 15000000; UP_CENA = 5000000;}
	        if(UpNow == 15000000) {UP_ID = 20000000; UP_CENA = 15000000;}
	        if(UpNow == 20000000) {UP_ID = -1; UP_CENA = -1;}
		}
		case 4:
	    {
	        if(UpNow == 10) {UP_ID = 20; UP_CENA = 5000000;}
	        if(UpNow == 20) {UP_ID = 30; UP_CENA = 10000000;}
	        if(UpNow == 30) {UP_ID = 40; UP_CENA = 20000000;}
	        if(UpNow == 40) {UP_ID = -1; UP_CENA = -1;}
	    }
	}
	return 1;
}
