stock AddRadek(const Texts[], pocet = 10)
{
	new STR[200], PismenoCount;
	format(STR, sizeof(STR), "%s", Texts);

	for(new i; i < strlen(STR); i ++)
	{
	    if(STR[i] == '{' && STR[i + 7] == '}') PismenoCount -= 8;
	    if(STR[i] == '~')
   		{
     		strdel(STR, i, i + 1);
       		strins(STR, "\n", i);

       		PismenoCount = 0;
    	}else{
			PismenoCount ++;
			if(PismenoCount > pocet)
			{
   				strins(STR,"\n", i, sizeof(STR));
			    PismenoCount = 0;
			}
		}
	}
	return STR;
}
