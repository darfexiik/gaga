    //--
	House[HouseID][NaProdej] = 0;
	House[HouseID][Lock] = 0;
	House[HouseID][H_Armour] = 0;
	House[HouseID][H_Health] = 0;
	House[HouseID][Money] = 0;
	House[HouseID][H_Spawn] = 0;
	House[HouseID][InteriorID] = 0;
	//--
	for(new i; i <MAX_HOUSE_ITEM; i ++)
	{
 		House[HouseID][Gun][i] = 0;
		House[HouseID][H_Ammo][i] = 0;
  		House[HouseID][H_Skin][i] = -1;
	}
    //--
	for(new i; i < HouseCarCount[HouseID]; i ++)
	{
		Veh_Paint[i][HouseID] 				= -1;
		Veh_Color[0][i][HouseID] 			= -1;
		Veh_Color[1][i][HouseID] 			= -1;
		Veh_Neon[i][HouseID] 				= -1;
		Veh_ID[i][HouseID] 					= 0;
		Veh_ID_Druh[i][HouseID] 			= 0;
		VehicleHouse[Veh_ID[i][HouseID]] 	= 0;
		Veh_ID_Hide[i][HouseID] 			= 0;
		format(Veh_SPZ[i][HouseID], 25, "Freeroam");

		format(Veh_Text[i][HouseID], 100, "");
		Veh_TextColor[i][HouseID] = -1;
		Veh_TextSize[i][HouseID] = 140;
		//--
		for(new ComID; ComID < MAX_HOUSE_COMPONENT; ComID ++)
		{
			Veh_Component[ComID][i][HouseID] = 0;
		}
		UlozHouseCar(HouseID,i);
	}
	//--
	DestroyPickup(House[HouseID][Pickup]);
	House[HouseID][Pickup] = CreatePickup(19522,1, House[HouseID][H_x], House[HouseID][H_y],House[HouseID][H_z]);
	Update3DTextHouse(HouseID);
	//---
 	UlozHouse(HouseID);
	return 1;
}
