public BoxMS(bx)
{
    if(Box[bx][Box_Zahajeni] > 0)
	{
 		SetTimerEx("BoxMS",1000,false,"i",bx);
		Box[bx][Box_Zahajeni] --;
		//----------------------------------------------------------------------
		switch(Box[bx][Box_Zahajeni])
		{
			case 1 .. 5:
			{
			    new string[50];
			    format(string, sizeof(string), "~r~%d", Box[bx][Box_Zahajeni]);
			    ForMaxPlayer(i)
				{
		  			if(Box[bx][Box_Hrac][i] == 1 && PlayerBox[i] == 1)
		    		{
						GameTextForPlayer(i, string, 1000, 3);
						TogglePlayerControllable(i, 0);
					 	PlayerPlaySound(i,1056,0.0,0.0,0.0);
					}
				}
			}
			case 0:
			{
			    ForMaxPlayer(i)
				{
		  			if(Box[bx][Box_Hrac][i] == 1 && PlayerBox[i] == 1)
		    		{
		    		    SetPlayerHealEx(i, 100);
						GameTextForPlayer(i, "~g~FIGHT!", 1000, 3);
						TogglePlayerControllable(i, 1);
						PlayerPlaySound(i,1057,0.0,0.0,0.0);
					}
					new string1[128];
					format(string1,sizeof(string1),"{FF9900}Box {FFFFFF}%s{FF9900} byl zahájen!", Box[bx][Box_Name]);
					SendClientMessage(i,0xC138FB88,string1);
				}
				if(Box[bx][Box_Prijmatel] == -1)
				{
				    ForMaxPlayer(i)
					{
			  			if(Box[bx][Box_Hrac][i] == 1 && PlayerBox[i] == 1)
			    		{
			    		    GivePlayerMoneyEx(i, Box[bx][Box_Cost]);
			    		    DSQ_Box(i);
			    		    SCM(i, 0xFF0000FF, "Box byl zrušen kvùli malému poètu úèastníkù!");
			    		}
			    		new string1[128];
						format(string1,sizeof(string1),"{FF9900}Box {FFFFFF}%s{FF9900} nikdo nevyhrál!", Box[bx][Box_Name]);
						SendClientMessage(i,0xC138FB88,string1);
					}
				    ZrusBox(bx);
				}else BoxTimer(bx);
			}
		}
		//----------------------------------------------------------------------
	}
	return 1;
}

