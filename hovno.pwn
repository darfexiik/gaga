CMD:npckick(playerid)
{
    if(strcmp(PlayerName(playerid), NPC_PRODEJ,false) == 0) return 1;
    if(IsPlayerNPC(playerid))
	{
	    if(IsPlayerConnected(playerid))
	    {
			Kick(playerid);
		}
	}
	return 1;
}

CMD:konechotel(playerid)
{
    if(IsPlayerNPC(playerid))
	{
	    RemovePlayerFromVehicle(playerid);
	    SetPlayerVirtualWorld(playerid, 1);
		new ID = -1;
	    //----
	    ForMaxPlayer(i)
		{
		    if(Hrac[IsLogin][i] == 1 && Hrac[Spawn][i] == 1 && MiseOn[i] == 1 && Hrac[Zamestnani][i] == HOTEL_SLUZBA)
		    {
                ID = i;
			}
		}
		//----
		if(ID != -1)
		{
		    for(new i; i < 12; i ++)
      		{
        		if(VehicleUSE[i] == 2)
          		{
                    SetVehicleParamsForPlayer(VehicleSAV[i], ID, 1, 0);
                    SetVehiclePos(VehicleSAV[i], 2034.6301,1910.3973,11.8018);
                    SetVehicleZAngle(VehicleSAV[i], 176.3622);
				}
			}
			SPIB2(ID,"Nasednete do ~y~vozidla",5);
		}else{
		    for(new i; i < 12; i ++)
    		{
	        	SetVehiclePosEx(VehicleSAV[i], Vehic_X[i],Vehic_Y[i], Vehic_Z[i]);
				SetVehicleZAngle(VehicleSAV[i], Vehic_A[i]);
				VehicleUSE[i] = 0;
				RepairVehicle(VehicleSAV[i]);
				SetVehicleHealth(VehicleSAV[i],999);
			}
			ServerDovozceOnly = 0;
		}
    }
	return 1;
}
