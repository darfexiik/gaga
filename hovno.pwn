stock DSQ_Box(playerid, ID = 0)
{
    new bx = GetPVarInt(playerid,"BoxID");
    if(Box[bx][Box_Hrac][playerid] == 1 && PlayerBox[playerid] == 1)
    {
        if(PlayerBoxDeath[playerid] == 0)
        {
            PlayerBoxDeath[playerid] = 0;
        	PlayerBox[playerid] = 0;
        	Box[bx][Box_Hrac][playerid] = 0;
        	SetPlayerVirtualWorldEx(playerid, 0);
        	SetPlayerArmourEx(playerid, PlayerDMArmour[playerid]);
        	Hrac[NPC_CP][playerid] = 2;
        	PlayerTextDrawHide(playerid, StadionTD[playerid]);
        	TogglePlayerControllable(playerid,1);

        	BackToport(playerid, 1);
        	Cheat[ZbraneProtekce][playerid] = 2;
			DejHnedGun(playerid);
        	if(Box[bx][Box_Created] == 1 && ID == 1)
        	{
	        	new Player_ID = Box[bx][Box_Vyzivatel];
				new Player_ID2 = Box[bx][Box_Prijmatel];
				new WinID, LoseID;
				//---
				if(playerid == Player_ID)
				{
					WinID = Player_ID2;
					LoseID = Player_ID;
				}
				if(playerid == Player_ID2)
				{
					WinID = Player_ID;
					LoseID = Player_ID2;
				}
				//---
				new string1[120];
				ForMaxPlayer(a)
				{
					format(string1,sizeof(string1),"{FF9900}Boxer {FFFFFF}%s {FF9900}vyhrál v boxu nad {FFFFFF}%s {FF9900}se skóre {FFFFFF}%d{FF9900}:{FFFFFF}%d{FF9900} o %s$", PlayerName(WinID), PlayerName(LoseID), PlayerBoxScore[WinID], PlayerBoxScore[LoseID], Number(Box[bx][Box_Cost] * 2));
	    			SendClientMessage(a,0xC138FB88,string1);
	    			Sav[VyhranoBoxu][WinID] ++;
				}
				if(Box[bx][Box_Cost] * 2 > 0) ODMENAF(100,WinID,5,"Vyhra~n~~w~~h~%s$",Number(Box[bx][Box_Cost] * 2));
				//---
				ZrusBox(bx);
				DSQ_Box(WinID);
			}

			PlayerBoxScore[playerid] = 0;
			PlayerTextDrawHide(playerid, StadionTD[playerid]);
		}
	}
	return 1;
}
