stock MemberInObject(FallID)
{
	new Members;
	if(FallObjectActive[FallID] == 1)
	{
	    new Float:ObjX, Float:ObjY, Float:ObjZ;
		GetObjectPos(FallObjectID[FallID], ObjX, ObjY, ObjZ);
	    ForMaxPlayer(i)
	    {
	        if(PlayerEvent[i] == 1)
	        {
	            if(IsPlayerInSphere(i, ObjX, ObjY, ObjZ, 4))
	            {
	                Members ++;
	            }
			}
		}
	}
	return Members;
}

stock SetEventPos(playerid)
{
    Cheat[PortyProtekce][playerid] = 3;

    switch(Event[EventDruh])
	{
	    case EVENT_FALLOUT:
	    {
		    new Float:ObjX, Float:ObjY, Float:ObjZ, x = random(MAX_FALL);

			GetObjectPos(FallObjectID[x], ObjX, ObjY, ObjZ);

			if(ObjX == 0.0000) return SetEventPos(playerid);
		    Cheat[PortyProtekce][playerid] = 3;

			SetPlayerPos(playerid, ObjX, ObjY, ObjZ + 2.0);
		}
		case EVENT_DERBY:
		{
		    new Float:PosX = FALL_OUT_X + 40.0, Float:PosY = FALL_OUT_Y - 7.0, ranExx = random(MAX_DERBY_CAR);

			if(DerbyVehUsed[ranExx] == 1) return SetEventPos(playerid);

            for(new i; i < 360; i += 20)
			{
				PosX += (14 * floatsin(-i, degrees));
				PosY += (14 * floatcos(-i, degrees));
                if((ranExx * 20) == i)
				{
					DerbyVeh[ranExx] = CreateVehicleEx(Event[EventZbran1], PosX, PosY, FALL_OUT_Z + 10.0, i + 15.0, -1, -1, -1);
					DerbyVehUsed[ranExx] = 1;
					PlayerEventCar[playerid] = ranExx;

		   			new engine,lights,alarm,doors,bonnet,boot,objective;
		   			GetVehicleParamsEx(DerbyVeh[ranExx], engine, lights, alarm, doors, bonnet, boot, objective);
		   			SetVehicleParamsEx(DerbyVeh[ranExx], 0, lights, alarm, doors, bonnet, boot, objective);

		   			PutPlayerInVehicleEx(playerid, DerbyVeh[ranExx], 0);

		   			TogglePlayerControllable(playerid, 0);
		   			return 1;
				}
			}
		}
	}
	return 1;
}

