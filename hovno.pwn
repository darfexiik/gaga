    || (IsPlayerInArea(playerid, 1928.9028, 2012.6154, -1221.4043, -1178.6207) && pz <= 18.45) //ls mitte teich
    || (IsPlayerInArea(playerid, 2295.7471, 2326.4858, -1431.1266, -1400.2797) && pz <= 22.615) //ls weiter südöstlich
    || (IsPlayerInArea(playerid, 2513.7588, 2550.0454, 1553.0753, 1583.3751) && pz <= 9.4171) //lv pool östlich
    || (IsPlayerInArea(playerid, 1087.3705, 1102.3634, -682.5446, -663.1653) && pz <= 112.45) //ls pool nordwestlich
    || (IsPlayerInArea(playerid, 1270.4369, 1287.7906, -810.0527, -801.3882) && pz <= 87.123) //pool bei maddog's haus oben
    || (pz < 1.5))
 	{
		return true;
 	}
 	return false;
}
