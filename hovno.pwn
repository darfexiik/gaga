stock PlayerMeters(playerid)
{
    if(IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
	    switch(GetVehicleType(GetPlayerVehicleID(playerid)))
		{
	 		case TYPE_CAR: Distance[DIST_Auto][playerid] += GetVehicleMeter(GetPlayerVehicleID(playerid));
	  		case TYPE_BOATS: Distance[DIST_Lod][playerid] += GetVehicleMeter(GetPlayerVehicleID(playerid));
	  		case TYPE_HELI: Distance[DIST_Heli][playerid] += GetVehicleMeter(GetPlayerVehicleID(playerid));
	    	case TYPE_LETADLA: Distance[DIST_Letadlo][playerid] += GetVehicleMeter(GetPlayerVehicleID(playerid));
		    case TYPE_MOTO: Distance[DIST_Moto][playerid] += GetVehicleMeter(GetPlayerVehicleID(playerid));
		    case TYPE_KAMION: Distance[DIST_Kamion][playerid] += GetVehicleMeter(GetPlayerVehicleID(playerid));
		    case TYPE_TRAIN: Distance[DIST_Vlak][playerid] += GetVehicleMeter(GetPlayerVehicleID(playerid));
		}
	}else{
	    new Kategorie1[50],Animace1[50];
		GetAnimationName(GetPlayerAnimationIndex(playerid),Kategorie1,50,Animace1,50);
	    if(!strcmp(Kategorie1,"SWIM", false ))
		{
  			if(!strcmp(Animace1,"SWIM_CRAWL", false ) || !strcmp(Animace1,"SWIM_BREAST", false ) || !strcmp(Animace1,"SWIM_TREAD", false ))
			{
   				Distance[DIST_Plavani][playerid] += GetPlayerMeter(playerid);
       			return 1;
			}
		}
	    Distance[DIST_Krok][playerid] += GetPlayerMeter(playerid);
	    AchUrazeno(playerid);
	}
	return 1;
}
