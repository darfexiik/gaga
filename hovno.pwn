stock PlayerPratele(playerid, const Friend[], IDSs)
{
    new Data[100], STR[50];
    format(Data, sizeof(Data), "Friends/%s.txt", PlayerName(playerid));

    if(fexist(Data))
	{
		switch(IDSs)
		{
		    case 1: //pøidání
		    {
				new File:FILE = fopen(Data,io_readwrite);
				while(fread(FILE, STR))
				{
				    new BName[MAX_PLAYER_NAME];
				    sscanf(STR,"s",BName);
				    strdel(BName, NajdiEnter(BName), strlen(BName));
					if(!strcmp(BName, Friend,false))
			 		{
						SCM(playerid, 0xFF0000FF, "Tento hráè již je ve Vaších pøátelých");
						fclose(FILE);
						return 1;
					}
				}
				fclose(FILE);
				//--------------------------------------------------------------
                new File:PFile = fopen(Data,io_append);
				format(Data,sizeof(Data),"%s\r\n", Friend);
				fwrite(PFile,Data);
				fclose(PFile);

				SCMF(100,playerid,0x3AF2FCFF,"Hráè %s byl pøidán mezi Vaše pøátele", Friend);
				//--------------------------------------------------------------
		    }
		    case 2://poèet pøátel
		    {
		        new CountPlayer;
		        new File:FILE = fopen(Data,io_readwrite);
				while(fread(FILE, STR))
				{
				    new BName[MAX_PLAYER_NAME];
				    sscanf(STR,"s",BName);
				    strdel(BName, NajdiEnter(BName), strlen(BName));

					new ID2 = GetPlayerID(BName);
			    	if(IsPlayerConnected(ID2))
				    {
                    	CountPlayer ++;
					}
				}
				fclose(FILE);
				return CountPlayer;
		    }
		    case 3: //seznam
		    {
		        format(MAX_STR,sizeof(MAX_STR),"");

		        new File:FILE = fopen(Data,io_readwrite);
				while(fread(FILE, STR))
				{
				    new BName[MAX_PLAYER_NAME];
				    sscanf(STR,"s",BName);
				    strdel(BName, NajdiEnter(BName), strlen(BName));

					new ID2 = GetPlayerID(BName);
			    	if(IsPlayerConnected(ID2))
				    {
                    	format(MAX_STR,sizeof(MAX_STR),"%s{84E812}%s (%d)\n", MAX_STR, BName, ID2);
					}else format(MAX_STR,sizeof(MAX_STR),"%s{FF0000}%s\n", MAX_STR, BName);
				}
				fclose(FILE);

				if(strlen(MAX_STR) > 0) ShowPlayerDialogEx(playerid, DIALOG_FRIENDS, DIALOG_STYLE_LIST, "Pøátele", MAX_STR, "Vybrat" ,"Zavøít");
				else					ShowPlayerDialogEx(playerid, INFORMACNI_DIALOG, DIALOG_STYLE_MSGBOX, "Pøátele", "Nemáte v seznamu žádné pøátelé", "Zavøít" ,"");
		    }
		}
	}else{
	    switch(IDSs)
	    {
	        case 1:
	        {
	            fcreate(Data);
	    		PlayerPratele(playerid, Friend, IDSs);
	        }
	        case 2: return 0;
	        case 3: ShowPlayerDialogEx(playerid, INFORMACNI_DIALOG, DIALOG_STYLE_MSGBOX, "Pøátele", "Nemáte v seznamu žádné pøátelé", "Zavøít" ,"");
	    }
	}
	return 1;
}
