stock CreateObjects(playerid)
{
	for(new i; i < ObjectsCount; i ++)
	{
		if(IsPlayerInSphere(playerid, Objects[i][Obj_X], Objects[i][Obj_Y], Objects[i][Obj_Z], MAX_OBJECT_VIEW))
  		{
  		    if(ObjView[playerid] < 900)
  		    {
	            if(Objects[i][ObjActiv][playerid] == 0)
		    	{
		    	    Objects[i][ObjPocet] = CreatePlayerObject(playerid, Objects[i][ObjID], Objects[i][Obj_X], Objects[i][Obj_Y], Objects[i][Obj_Z], Objects[i][rObj_X], Objects[i][rObj_Y], Objects[i][rObj_Z], MAX_OBJECT_VIEW);
		    	    Objects[i][ObjActiv][playerid] = 1;
		    	    ObjView[playerid] ++;
		    	}
			}else{
			    ObjectDistanceDestroy(playerid);
			    //---------------------
			    if(Objects[i][ObjActiv][playerid] == 0)
		    	{
		    	    Objects[i][ObjPocet] = CreatePlayerObject(playerid, Objects[i][ObjID], Objects[i][Obj_X], Objects[i][Obj_Y], Objects[i][Obj_Z], Objects[i][rObj_X], Objects[i][rObj_Y], Objects[i][rObj_Z], MAX_OBJECT_VIEW);
		    	    Objects[i][ObjActiv][playerid] = 1;
		    	    ObjView[playerid] ++;
		    	}
			}
	    }else{
            if(Objects[i][ObjActiv][playerid] == 1)
	    	{
	    	    DestroyPlayerObject(playerid, Objects[i][ObjPocet]);
	    	    Objects[i][ObjActiv][playerid] = 0;
	    	    ObjView[playerid] --;
	    	}
	    }
	}
	return 1;
}

stock ObjectDistanceDestroy(playerid)
{
	new Float:Distance1, Float:MaxDistance, ObjectIDs;
    for(new i; i < ObjectsCount; i ++)
	{
	    if(Objects[i][ObjActiv][playerid] == 1)
    	{
    	    Distance1 = GetPlayerDistanceFromPoint(playerid, Objects[i][Obj_X], Objects[i][Obj_Y], Objects[i][Obj_Z]);
  		    if(Distance1 > MaxDistance)
  		    {
  		        ObjectIDs = i;
  		        MaxDistance = Distance1;
  		    }
		}
	}
	DestroyPlayerObject(playerid, Objects[ObjectIDs][ObjPocet]);
 	Objects[ObjectIDs][ObjActiv][playerid] = 0;
 	ObjView[playerid] --;
	return 1;
}
