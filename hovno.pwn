CMD:giveammo(playerid, params[])
{
    new id = strval(params),ammo;
    if(sscanf(params,"ui",id,ammo)) 						return SCM(playerid,0xFF0000AA,"Nápovìda: /giveammo (ID) (POÈET NÁBOJÙ)");
    else if(!IsPlayerConnected(id)) 						return SCM(playerid,0xFF0000AA,"Hráè není na serveru");
    else if(GetPlayerAmmo(playerid) < ammo || ammo > GetPlayerAmmo(playerid)) return SCM(playerid,0xFF0000AA,"Špatný parametr!");
    else if(ammo < 0) 										return SCM(playerid,0xFF0000AA,"Špatný parametr!");
	else if(PlayerInDM[playerid] == 1)						return SCM(playerid,0xFF0000AA,"Z deathmatche nelze poslat náboje!");
	else if(IsPlayerNPC(id)) 								return SCM(playerid,0xFF0000AA,"Zadané ID má NPC!");
    else if(PlayerBox[id] == 1)								return SCM(playerid,0xFF0000AA,"Tento hráè je v minihøe");
    else if(zavod[id] == 1)									return SCM(playerid,0xFF0000AA,"Tento hráè je v minihøe");
	else if(IsPlayerInAnyVehicle(playerid))					return SCM(playerid,0xFF0000AA,"Ve vozidle nelze poslat zbranì");
	else if(PlayerInDM[id] == 1)							return SCM(playerid,0xFF0000AA,"Tento hráè je v minihøe");
	else if(playerid == id)                                 return SCM(playerid,0xFF0000AA,"Špatný parametr!");
	else if(!IsPlayerStreamedIn(playerid, id))              return SCM(playerid,0xFF0000AA,"Hráè je pøíliš daleko!");
	else if(SeskokPlayer[id] == 1)                          return SCM(playerid,0xFF0000AA,"Tento hráè je v minihøe");
	else if(PlayerBlokAmmo[id][playerid] == 1)    			return SCM(playerid,0xff0000AA,"Tento hráè si zablokoval pøíjem zbraní od Vás");

    Cheat[ZbraneProtekce][playerid] = 5;
    Cheat[ZbraneProtekce][id] = 5;

	GivePlayerWeaponEx(id,GetPlayerWeapon(playerid),ammo);

	if(GetPlayerAmmo(playerid) == ammo) SetPlayerAmmo(playerid,GetPlayerWeapon(playerid),-1);
	SetPlayerAmmo(playerid,GetPlayerWeapon(playerid),GetPlayerAmmo(playerid) -ammo);
	//GivePlayerWeaponEx(id,GetPlayerWeapon(playerid),-ammo);

	SCMF(100,id,0x00FFFFAA,"Hráè %s (%d) Vám poslal %s s %d náboji",PlayerName(playerid),playerid,WeaponName(GetPlayerWeapon(playerid)),ammo);
	SCMF(100,playerid,0x00FFFFAA,"Poslal jste hráèi %s (%d) %s s %d náboji",PlayerName(id),id,WeaponName(GetPlayerWeapon(playerid)),ammo);

    Log_Write(100,id,"(AMMO) Hráè %s (%d) Vám poslal %s s %d náboji",PlayerName(playerid),playerid,WeaponName(GetPlayerWeapon(playerid)),ammo);
    Log_Write(100,playerid,"(AMMO) Poslal jste hráèi %s (%d) %s s %d náboji",PlayerName(id),id,WeaponName(GetPlayerWeapon(playerid)),ammo);
	return true;
}

