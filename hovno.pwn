stock TuningView(playerid, Component1)
{
	if(Component1 == VIEW_ZAKLAD) return SetCameraBehindPlayer(playerid);

    new ID = GetVehicleComponentType(Component1);
	new IDs = ViewID(ID);
    new Float: x, Float: y, Float:z;
    GetVehiclePos(GetPlayerVehicleID(playerid), x, y, z);
	switch(IDs)
	{
	    case 8: SetCameraBehindPlayer(playerid);
		default:
		{
			SetPlayerCameraPos(playerid, TuningCords[IDs][0] + x, TuningCords[IDs][1] + y, TuningCords[IDs][2] + z);
			SetPlayerCameraLookAt(playerid, TuningCords[IDs][3] + x, TuningCords[IDs][4] + y, TuningCords[IDs][5] + z);
		}
	}
	PlayerDialogTuning[playerid] = ID;
	return 1;
