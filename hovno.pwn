CMD:givecash(playerid, params[])
{
	new Castka,id = strval(params[0]);
	if(sscanf(params,"ui",id,Castka)) 					return SCM(playerid,0xff0000AA,"Nápovìda: /givecash (ID) (ÈÁSTKA)");
	else if(GetPlayerMoneyEx(playerid) < Castka) 		return SCM(playerid,0xff0000AA,"Pro tento pøíkaz nemáte dostatek penìz!");
	else if(!IsPlayerConnected(id)) 					return SCM(playerid,0xff0000AA,"Vámi zadané ID není pøipojeno na serveru!");
	else if(Castka < 0  || Castka == 0) 				return SCM(playerid,0xff0000AA,"Chybná èástka!");
	else if(IsPlayerNPC(id)) 							return SCM(playerid,0xff0000AA,"Zadané ID má NPC!");
	else if(Sav[Minuty][playerid] < 300) 				return SCM(playerid,0xff0000AA,"Poslat peníze mùžete až budete mít nahráno 5 hodin");
	else if(id == playerid) 							return SCM(playerid,0xff0000AA,"Nemùžeš poslat peníze sám sobì!");

	GivePlayerMoneyEx(playerid, -Castka);
	GivePlayerMoneyEx(id, Castka);

	SCMF(128,id,0xFF0000AA,"Hráè %s (%d) Vám poslal èástku %s$",PlayerName(playerid),playerid,Number(Castka));
	SCMF(128,playerid,0xFF0000AA,"Poslal jste hráèi %s (%d) èástku %s$",PlayerName(id),id,Number(Castka));

    Log_Write(100,id,"(MONEY) Hráè %s (%d) Vám poslal èástku %s$",PlayerName(playerid),playerid,Number(Castka));
    Log_Write(100,playerid,"(MONEY) Poslal jste hráèi %s (%d) èástku %s$",PlayerName(id),id,Number(Castka));
	return 1;
}
