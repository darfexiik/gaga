stock CreateMafieCar()
{
	for(new MafieID = 0; MafieID  < MafieCount; MafieID ++)
	{
		for(new ID; ID < MAX_MAFIE_CAR; ID ++)
		{
		    //--
		    MafVeh_ID[ID][MafieID] 			= 0;
		    MafVeh_ID_Druh[ID][MafieID] 	= 0;
		    format(MafVeh_Text[ID][MafieID], 100, "");
			MafVeh_TextColor[ID][MafieID] = -1;
			MafVeh_TextSize[ID][MafieID] = 140;
		    //--
		    new string[50], Data[50];
			format(string, sizeof(string), "Mafie/Mafie%dCar%d.txt", MafieID, ID);
			if(!fexist(string))
			{
			    new File:PFile = fopen(string,io_write);
			    format(Data,sizeof(Data),"%d\r\n",MafVeh_ID_Druh[ID][MafieID]);				fwrite(PFile,Data);
			    //---
			    format(Data,sizeof(Data),"%f\r\n",Veh_MX[ID][MafieID]);						fwrite(PFile,Data);
			    format(Data,sizeof(Data),"%f\r\n",Veh_MY[ID][MafieID]);						fwrite(PFile,Data);
			    format(Data,sizeof(Data),"%f\r\n",Veh_MZ[ID][MafieID]);						fwrite(PFile,Data);
			    format(Data,sizeof(Data),"%f\r\n",Veh_MA[ID][MafieID]);						fwrite(PFile,Data);
			    //---
			    for(new i; i < MAX_MAFIE_COMPONENT; i ++)
			    {
			        format(Data,sizeof(Data),"%d\r\n",MafVeh_Component[i][ID][MafieID]);	fwrite(PFile,Data);
				}

				format(Data,sizeof(Data),"%s\r\n",MafVeh_Text[ID][MafieID]);				fwrite(PFile,Data);
				format(Data,sizeof(Data),"%d\r\n",MafVeh_TextColor[ID][MafieID]);			fwrite(PFile,Data);
				format(Data,sizeof(Data),"%d\r\n",MafVeh_TextSize[ID][MafieID]);			fwrite(PFile,Data);
			    fclose(PFile);
		    }else{
		        new File:PFile = fopen(string,io_readwrite);
				fread(PFile,Data,sizeof(Data)); MafVeh_ID_Druh[ID][MafieID]					= strval(Data);
				//---
				fread(PFile,Data,sizeof(Data)); Veh_MX[ID][MafieID]  						= strval(Data);
				fread(PFile,Data,sizeof(Data)); Veh_MY[ID][MafieID]  						= strval(Data);
				fread(PFile,Data,sizeof(Data)); Veh_MZ[ID][MafieID]  						= strval(Data);
				fread(PFile,Data,sizeof(Data)); Veh_MA[ID][MafieID]  						= strval(Data);
				//---
			    for(new i; i < MAX_MAFIE_COMPONENT; i ++)
			    {
			        fread(PFile,Data,sizeof(Data)); MafVeh_Component[i][ID][MafieID]		= strval(Data);
				}
				//--
				fread(PFile,Data,sizeof(Data));
	 			strdel(Data,NajdiEnter(Data),strlen(Data));
	 			//--
				format(MafVeh_Text[ID][MafieID], MAX_PLAYER_NAME, Data);

				fread(PFile,Data,sizeof(Data)); MafVeh_TextColor[ID][MafieID]  				= strval(Data);
				fread(PFile,Data,sizeof(Data)); MafVeh_TextSize[ID][MafieID]  				= strval(Data);

				fclose(PFile);

				if(MafVeh_ID_Druh[ID][MafieID] > 0)
				{
		    		CarMafieCreate(MafieID, ID, 0);
				}
		    }
			//---
		    MafieCarCount[MafieID] ++;
		}
	}
	return 1;
}
