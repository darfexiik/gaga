stock CreateVehicleEx(VehType, Float:Veh_x, Float:Veh_y, Float:Veh_z, Float:Veh_a, color1, color2, respawn = -1)
{
	new ID;
    ID = CreateVehicle(VehType, Float:Veh_x, Float:Veh_y, Float:Veh_z, Float:Veh_a, color1, color2, respawn);
    SetVehicleHealth(ID,999);
    //--
    CreatedCar[ID] = 1;
    SetVehicleNumberPlate(ID, "Realny zivot 1.0.5");
    ZamekCar[ID] = 0;
    format(VehicleLocName[ID],24,"");
    //--
    DestroyObject(Neon2[ID]);
	DestroyObject(Neon[ID]);
	DestroyObject(VehText[ID]);
	DestroyObject(VehText2[ID]);

	VehicleTextTuningColor[ID] = -1;
	VehicleTuningColorLast[ID] = -1;
	format(VehicleTextTuningSTR[ID], 100, "");
	format(VehicleTuningSTRLast[ID], 100, "");
	VehicleTextTuningSize[ID] = 140;
	VehicleTuningSizeLast[ID] = 140;
    //--
    VehiclePosX[ID] = Veh_x;
 	VehiclePosY[ID] = Veh_y;
 	VehiclePosZ[ID] = Veh_z;
 	VehicleColor[0][ID] = color1;
	VehicleColor[1][ID] = color2;
	VehiclePaint[ID] = -1;
 	NaplnitKanistrVozuID(ID, 100);
 	//--
 	for(new i; i < 17; i ++) VehicleTune[i][ID] = 0;
    //--
    DisableSipka(ID);
    //--
    new engine,lights,alarm,doors,bonnet,boot,objective;
	GetVehicleParamsEx(ID,engine,lights,alarm,doors,bonnet,boot,objective);
	SetVehicleParamsEx(ID,1,0,0,0,0,0,objective);
