CMD:hitman(playerid,params[])
{
    new id = strval(params[0]),cast;
    if(sscanf(params,"ii",id,cast)) 												return SCM(playerid,0xff0000AA,"Nápovìda: /hitman (ID) (ÈÁSTKA)");
	else if(GetPlayerMoneyEx(playerid) < cast || cast > GetPlayerMoneyEx(playerid)) return SCM(playerid,0xff0000AA,"Nemáte tolik penìz");
	else if(cast < 1) 																return SCM(playerid,0xff0000AA,"Musíte vypsat minimálnì 1$.");
	else if(!IsPlayerConnected(id)) 												return SCM(playerid,0xff0000AA,"Hráè není na serveru");
	else if(IsPlayerNPC(id)) 														return SCM(playerid,0xff0000AA,"Nemùžete vypsat peníze na NPC!");

	Sav[HitmanCastka][id] += cast;

	GivePlayerMoneyEx(playerid, -cast);

	if(Sav[PruvodceUkol][playerid] == P_HITMAN) Pruvodce[P_HITMAN][playerid] = 1;

    ForMaxPlayer(i)
    {
    	switch(Set[S_Hitman][i])
	    {
       		case 1: AddChat(100, i,"~w~Hrac ~r~%s ~w~vypsal odmenu ~g~%s$ ~w~na ~r~%s~w~ celkova castka je ~g~%s$",PlayerName(playerid),Number(cast),PlayerName(id),Number(Sav[HitmanCastka][id]));
       		case 2: AddChat(100, i,"~w~Hrac ~r~%s ~w~vypsal odmenu ~g~%s$ ~w~na ~r~%s~w~ celkova castka je ~g~%s$",PlayerName(playerid),Number(cast),PlayerName(id),Number(Sav[HitmanCastka][id]));
	    }
	 }
	return 1;
}

