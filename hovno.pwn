CMD:vyhodit(playerid, params[])
{
    new id = strval(params);
	if(sscanf(params,"u",id)) 										return SCM(playerid,0xFF0000AA,"Nápovìda: /vyhodit [ID]");
	else if(IsPlayerNPC(id)) 										return SCM(playerid,0xFF0000AA,"Zadaní ID má NPC!");
    else if(!IsPlayerConnected(id)) 								return SCM(playerid,0xFF0000AA,"Hráè není na serveru");
    else if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) 		return SCM(playerid,0xFF0000AA,"Musíte být øidiè");
	else if(!IsPlayerInAnyVehicle(id)) 								return SCM(playerid,0xFF0000AA,"Tento hráè nesedí v žádném vozidle");
