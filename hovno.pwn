stock SeradDMTop(dm)
{
    for(new i; i < 5; i ++) DM_TOP[i][dm] = -1;
    //--------------------------------------------------------------------------
    ForMaxPlayer(i)
	{
	    format(DM_STR_TOP[i], 200, "");
		if(DM[dm][InDM][i] == 1 && PlayerInDM[i] == 1)
		{
		    new Kolik;
            ForMaxPlayer(x)
			{
				if(DM[dm][InDM][x] == 1 && PlayerInDM[x] == 1)
  				{
  				    //----------------------------------------------------------
  				    if(DMP_PlayerScore[i] > DMP_PlayerScore[x])
  				    {
  				        Kolik ++;
  				    }
  				    //----------------------------------------------------------
  				    if(DMP_PlayerScore[i] == DMP_PlayerScore[x])
  				    {
  				        if(DMP_ScoreTimer[i] < DMP_ScoreTimer[x])
  				        {
  				            Kolik ++;
  				        }
  				    }
  				    //----------------------------------------------------------
  				}
			}
			new Misto = DM[dm][DM_Players] - Kolik;
			//------------------------------------------------------------------
			DM_TOP[Misto - 1][dm] = i;
			//------------------------------------------------------------------
		}
	}
	//--------------------------------------------------------------------------
	new Count_D;
	if(DM[dm][DM_Players] > 5) 	Count_D = 5;
	else                        Count_D = DM[dm][DM_Players];
	//--------------------------------------------------------------------------
	ForMaxPlayer(i)
	{
		if(DM[dm][InDM][i] == 1 && PlayerInDM[i] == 1)
		{
			for(new x; x < Count_D; x ++)
			{
			    new ID = DM_TOP[x][dm];
			    if(strcmp(PlayerName(i), PlayerName(ID), false) == 0)			format(DM_STR_TOP[i], 200, "%s~g~~h~%d %s %d.~n~", DM_STR_TOP[i], DMP_PlayerScore[ID], PlayerName(ID), x + 1);
			    else                                                            format(DM_STR_TOP[i], 200, "%s~w~%d %s %d.~n~", DM_STR_TOP[i], DMP_PlayerScore[ID], PlayerName(ID), x + 1);
			}
		}
	}
	return 1;
}
