  				            DM[dm][DM_Score] = DMP_PlayerScore[i];
							DM[dm][DM_ASI_WIN_ID] = i;
  				        }
					}
					//----------------------------------------------------------
					DSG_DM(i);
				}
			}
		}
		//----------------------------------------------------------------------
		DM[dm][DM_Opak] = 0;
		DM[dm][DM_Created] = 0;
