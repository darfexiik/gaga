CMD:event(playerid)
{
    if(PlayerEvent[playerid] == 1)              return SCM(playerid, 0xFF0000FF, "Již jste na eventu");
    else if(Event[EventPortON] == 0)            return SCM(playerid, 0xFF0000FF, "Port na event není povolen");
//    else if(Sav[Jailed][playerid] == 1)         return SCM(playerid, 0xFF0000FF, "Príkaz nelze použi vo vezení");

    ResetGun(playerid);
	BackToport(playerid, 0);

	new randoms, Team1 = GetMemberTeam(1), Team2 = GetMemberTeam(2);
 	switch(Event[EventDruh])
  	{
   		case EVENT_DM:
     	{
      		Cheat[PortyProtekce][playerid] = 3;

			randoms = random(MAX_EVENT_SPAWN);
			SetPlayerPos(playerid, Event[EventSpawnDM_X][randoms], Event[EventSpawnDM_Y][randoms], Event[EventSpawnDM_Z][randoms]);

			SetPlayerInterior(playerid, Event[EventInt]);
			SetPlayerVirtualWorldEx(playerid, EVENT_VW);
		}
 		case EVENT_TEAM_DM:
		{
  			Cheat[PortyProtekce][playerid] = 3;
  			if(Team2 > Team1 || Team1 == 0)
  			{
  				PlayerEventTeam[playerid] = 1;
   				SetPlayerPos(playerid, Event[EventSpawnDM_X][0], Event[EventSpawnDM_Y][0], Event[EventSpawnDM_Z][0]);
     			SetPlayerColor(playerid, 0xFF0000FF);
       		}
        	if(Team1 > Team2)
       		{
        		PlayerEventTeam[playerid] = 2;
				SetPlayerPos(playerid, Event[EventSpawnDM_X][1], Event[EventSpawnDM_Y][1], Event[EventSpawnDM_Z][1]);
   				SetPlayerColor(playerid, 0xFF0000FF);
       		}
         	SetPlayerInterior(playerid, Event[EventInt]);
         	SetPlayerVirtualWorldEx(playerid, EVENT_VW);
		}
       	case EVENT_FALLOUT: SetEventPos(playerid);
		case EVENT_PLOSINY:
		{
  			Cheat[PortyProtekce][playerid] = 3;
  			new Float:ObjX, Float:ObjY, Float:ObjZ, x = random(MAX_PLOSINY);
			GetObjectPos(FallObjectID[x], ObjX, ObjY, ObjZ);

			SetPlayerPos(playerid, ObjX, ObjY, ObjZ + 2.0);
			SetPlayerFacingAngle(playerid, 0.0);
		}
		case EVENT_DERBY: SetEventPos(playerid);
		case EVENT_BLUDISTE:
		{
		    switch(random(3))
		    {
		        case 0: SetPlayerPosEx(playerid, 875.8060,3685.1499,155.5174,272.4934);
				case 1: SetPlayerPosEx(playerid, 877.2759,3685.1633,155.5174,270.4881);
				case 2: SetPlayerPosEx(playerid, 879.3679,3685.1819,155.5174,270.4881);
			}
		}
	}
	SetCameraBehindPlayer(playerid);
	PlayerEvent[playerid] = 1;
	return 1;
}

