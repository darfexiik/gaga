public CarMafieCreate(MafieID, CountCar, ID)
{
    if(ID == 0)		MafVeh_ID[CountCar][MafieID] = CreateVehicleEx(MafVeh_ID_Druh[CountCar][MafieID],Veh_MX[CountCar][MafieID], Veh_MY[CountCar][MafieID], Veh_MZ[CountCar][MafieID], Veh_MA[CountCar][MafieID], Mafie[MafieID][MafieBarvaAut], Mafie[MafieID][MafieBarvaAut], 180);
	if(ID == 1)     SetVehicleToRespawn(MafVeh_ID[CountCar][MafieID]);

	ChangeVehiclePaintjob(MafVeh_ID[CountCar][MafieID], -1);
	ChangeVehicleColor(MafVeh_ID[CountCar][MafieID], Mafie[MafieID][MafieBarvaAut], Mafie[MafieID][MafieBarvaAut]);

	for(new ComID; ComID < MAX_MAFIE_COMPONENT; ComID ++)
 	{
    	AddVehicleComponent(MafVeh_ID[CountCar][MafieID], MafVeh_Component[ComID][CountCar][MafieID]);
	}
	//---
	SetVehicleNumberPlate(MafVeh_ID[CountCar][MafieID], Mafie[MafieID][MafieName]);

   	LinkVehicleToInterior(MafVeh_ID[CountCar][MafieID], 0);
	SetVehicleVirtualWorld(MafVeh_ID[CountCar][MafieID], 0);

    new VehID = MafVeh_ID[CountCar][MafieID];

    if(VehicleText[VehID] == 1)
    {
		DestroyObject(VehText[VehID]);
		DestroyObject(VehText2[VehID]);
	}

    if(strlen(MafVeh_Text[CountCar][MafieID]) > 0)
   	{
   		new model = GetVehicleModel(VehID);

		VehText[VehID] = CreateObject(19482,0,0,0,0,0,0);

  		AttachObjectToVehicle(VehText[VehID], VehID, VehOffSet[model-400][0], VehOffSet[model-400][1], VehOffSet[model-400][2],0,0,180.0);
    	SetObjectMaterialText(VehText[VehID], AddRadek(MafVeh_Text[CountCar][MafieID]) , 0, MafVeh_TextSize[CountCar][MafieID], "Arial", 32, 1, MafVeh_TextColor[CountCar][MafieID],0,OBJECT_MATERIAL_TEXT_ALIGN_CENTER);

		VehText2[VehID] = CreateObject(19482,0,0,0,0,0,0);
  		AttachObjectToVehicle(VehText2[VehID], VehID, -VehOffSet[model-400][0], VehOffSet[model-400][1], VehOffSet[model-400][2],0,0,0.0);
    	SetObjectMaterialText(VehText2[VehID], AddRadek(MafVeh_Text[CountCar][MafieID]), 0, MafVeh_TextSize[CountCar][MafieID]," Arial", 32, 1, MafVeh_TextColor[CountCar][MafieID],0,OBJECT_MATERIAL_TEXT_ALIGN_CENTER);

        VehicleTextTuningSize[VehID] = MafVeh_TextSize[CountCar][MafieID];
        VehicleTextTuningColor[VehID] = MafVeh_TextColor[CountCar][MafieID];
		format(VehicleTextTuningSTR[VehID], 100, "%s", MafVeh_Text[CountCar][MafieID]);

		VehicleText[VehID] = 1;
   	}

   	VehicleCanPark[VehID] = 1;
	//----
	GetVehiclePos(VehID, VehiclePosX[VehID], VehiclePosY[VehID], VehiclePosZ[VehID]);
	return 1;
}

