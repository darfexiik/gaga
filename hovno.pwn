			    			strins(string, STR_Name, i, 128);
		    	  		}
	     			}
		      	}
			}
		}

		TextDrawSetString(Dann,TextHack(string));

		ForMaxPlayer(i)
		{
		    if(SPIB2_On[i] == 0)
		    {
		        TextDrawShowForPlayer(i, Dann);
		    }
		}
		//TextDrawShowForAll(Dann);
		KillTimer(DannTimer);
		DannTimer = SetTimer("DannHide",cas*1000,0);
	}else SCM(playerid,0xFFFFFFAA,"SERVER: Pøíkaz neexistuje -> /help nebo /prikazy");
 	return 1;
}

