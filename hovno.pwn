public SecondTimer()
{
    PlayerMinut ++;
    if(PlayerMinut >= 60)
    {
        PlayerMinut = 0;
        PlayerHodin ++;
        if(PlayerHodin >= 24)
        {
            PlayerHodin = 0;
        }
    }

	//ForMaxPlayer(playerid)
	for(new playerid = 0; playerid <MaxPlayerss + 20; playerid ++)
	{
		if(IsPlayerConnected(playerid))
		{
		    if(!IsPlayerNPC(playerid))
		    {
				//-----
			    CreateObjects(playerid);
			    if(Hrac[IsLogin][playerid] == 1 && Hrac[Spawn][playerid] > 0)
				{
				    PlayerMeters(playerid);
				    MiseSecond(playerid);
				    //----------------------------------------------------------
				    if(AZ[Z_Stav] == 1)
				    {
					    if(AZ[Z_Typ] == 1)
						{
						    if(AZ[Z_PlayerID] == playerid)
						    {
						        CreateAdminZone(playerid);
							}
						}
					}
				    //----------------------------------------------------------
				    SetPlayerTime(playerid, PlayerHodin, PlayerMinut);
				    SetPlayerScore(playerid, GetPlayerMoney(playerid));
				    //----------------------------------------------------------
					if(	IsPlayerInArea(playerid,  1749.8782, 1813.9310, -1728.3934, -1676.0796) ||
						IsPlayerInArea(playerid,  -1994.5334, -1908.3115, 551.3755, 624.5874) 	||
						IsPlayerInArea(playerid,  2072.4409, 2242.1787, 1373.1871, 1482.8796))
					{
				  		if(GetPlayerWeapon(playerid) != 0)
				   		{
				     		if(GetPlayerVirtualWorld(playerid) == 0)
			 				{
						 		SetPlayerArmedWeapon(playerid,0);
							}
						}
						switch(GetVehicleModel(GetPlayerVehicleID(playerid)))
		    			{
	        				case 425, 432, 447, 520:
				        	{
	            				SPIB(playerid, "~r~Vojenska technika je u warpu zakazana!", 3);
					            RemovePlayerFromVehicle(playerid);
					        }
					    }
					}
					//----------------------------------------------------------
					if(GetPlayerInterior(playerid) > 0)
					{
						for(new i; i < MAX_DRUH_STROJE; i ++)
						{
							if(	(IsPlayerInRangeOfPoint(playerid, 2, BikePost[i][0], BikePost[i][1], BikePost[i][2]) && BikeUsed[i] == 0) 		||
								(IsPlayerInRangeOfPoint(playerid, 2, PasPost[i][0], PasPost[i][1], PasPost[i][2]) && PasUsed[i] == 0)			||
								(IsPlayerInRangeOfPoint(playerid, 2, BenchPost[i][0], BenchPost[i][1], BenchPost[i][2]) && BenchUsed[i] == 0)	||
								(IsPlayerInRangeOfPoint(playerid, 2, CinkyPost[i][0], CinkyPost[i][1], CinkyPost[i][2]) && CinkyUsed[i] == 0))
							{
								SPIB(playerid, "Stisknete RETURN pro cviceni", 1);
							}
						}
					}
					//----------------------------------------------------------
					CheckPruvodce(playerid);
					//PovoHodnost(playerid);

					if(Hrac[AFK][playerid] == 1 && Hrac[AFKAway][playerid] > 20) SPIB(playerid, "Pro odchod z AFK stisknete RETURN", 5);
					//----------------------------------------------------------
				    if(IsPlayerInAnyVehicle(playerid))
				    {
				        new VehID = GetPlayerVehicleID(playerid);
				        GetVehiclePos(VehID, VehiclePosX[VehID], VehiclePosY[VehID], VehiclePosZ[VehID]);
				    }
				    //----------------------------------------------------------
				    if(PlayerInDM[playerid] == 1)               						Cheat[ZbraneProtekce][playerid] = 5;
				    if(	IsPlayerInSphere(playerid, 1572.7806,-1335.3168,16.4844, 10) ||
						IsPlayerInSphere(playerid, 1548.3837,-1366.3110,326.2109, 10) ||
						Spectate[playerid] != -1) 	Cheat[ActiveAntiCheat][playerid] = 1;
				    //----------------------------------------------------------
					if(Cheat[ActiveAntiCheat][playerid] == 5)
					{
						AntiHack(playerid);
					}
					//----------------------------------------------------------
					CheckAdminInfo(playerid);
				    //----------------------------------------------------------
		      		GetPlayerPos(playerid, Player_Pos[0][playerid] ,Player_Pos[1][playerid] ,Player_Pos[2][playerid]);
		      		//----------------------------------------------------------
					if(PlayerEvent[playerid] == 1)
					{
					    if(Player_Pos[2][playerid] >= FALL_OUT_Z - 15)
					    {
                            SetPlayerArmedWeapon(playerid, Event[EventZbran1]);
						}else{
						    if(Event[EventDruh] == EVENT_FALLOUT || Event[EventDruh] == EVENT_PLOSINY || Event[EventDruh] == EVENT_DERBY)
						    {
						        EventEnd(playerid);
							}
