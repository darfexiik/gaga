stock DeleteLineFromLine(file[],linea)
{
	new File:a = fopen(file,io_readwrite);
	new str[128],str2[128];
	format(str2,128,"%s.part",file);
	new File:b = fopen(str2,io_write);
	new lineb = 1;
	while(fread(a,str))
	{
		if(lineb == linea)
		{
	 		lineb++;
	 		continue;
		}else{
		fwrite(b,str);
	}
		lineb ++;
	}
	fclose(a);
	fremove(file);
	fclose(b);
	b = fopen(str2,io_readwrite);
	fcreate(file);
	a = fopen(file,io_write);
	while(fread(b,str))
	{
		fwrite(a,str);
	}
	fclose(a);
	fclose(b);
	fremove(str2);
}
