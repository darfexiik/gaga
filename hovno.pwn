public OnPlayerCommandPerformed(playerid, cmdtext[], success)
{
    if(Hrac[IsLogin][playerid] == 1)
	{
	    format(MAX_STR,sizeof(MAX_STR),"");
        if(cmdtext[1]=='/' && IsPlayerAdmin(playerid) || cmdtext[1]=='/' && Sav[Admin][playerid] > 0)
		{
			new string[128];
			strmid(string,cmdtext,2,strlen(cmdtext));
			//------------------------------------------------------------------
		  	for (new i; i < 128; i++)
		  	{
		   		if(string[i] == '#')
		   		{
		     		if ((string[i + 1] > 47) && (string[i + 1] < 58))
				 	{
			      		new id = StrvalEx(string[i + 1]);
			        	if (id <= 500)
						{
			   	  			if (IsPlayerConnected(id) && !IsPlayerNPC(id))
					 		{
				    			new STR_Name[70];
								new j = i + 1;
								if(id < 10) j += 1;
								else if(id < 100) j += 2;
								else if(id < 1000) j += 3;

				    			strdel(string, i, j);
				    			format(STR_Name, sizeof(STR_Name), "{%06x}%s{03D003}",(GetPlayerColor(id) >>> 8), PlayerName(id));
				    			strins(string, STR_Name, i, sizeof(string));
			    	  		}
		     			}
			      	}
				}
			}
	        //------------------------------------------------------------------
			SCMAT(128,0x03D003AA,"[Admin Chat] %s (%d): %s", PlayerName(playerid),playerid, string);
			//------------------------------------------------------------------
			return 1;
		}
		//------------------------------------------------------------------
		if(success == 1)
		{
		    if(strfind(cmdtext, "mojecmd", true) == -1)
	  		{
				new str[127];
				format(str,127,"[CMD] %s (%d): %s",PlayerName(playerid),playerid,cmdtext);
				ForMaxPlayer(i)
				{
			 		if (IsPlayerAdmin(i) || Sav[Admin][i] > 0)
			   		{
			   		    if(CmdOn[i] == 1)
			   		    {
			   		        if(i != playerid) SendClientMessage(i, 0x7E6A22AA, str);
						}
			     	}
				}
				Log_Write(100,playerid,"(CMD) %s: %s", PlayerName(playerid),cmdtext);
			}
		}
		//------------------------------------------------------------------
	}
	if(success == 0)return SCM(playerid,0xFFFFFFAA,"SERVER: Pøíkaz neexistuje -> /help nebo /prikazy");
    return 1;//If player writes command wrong this will still return it as a true.
}

